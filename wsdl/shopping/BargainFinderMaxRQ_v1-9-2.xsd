				<xs:documentation xml:lang="en">Class of service</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Shopped" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specify whether the flight is shopped.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<!-- END CurrentOriginDestinationFlightExchangeAttributeGroup -->
	<xs:attributeGroup name="ExchangeFareAttributeGroup">
