			<xs:element name="BookingChannel" type="SourceBookingChannelType" minOccurs="0"/>
		</xs:sequence>
		<xs:attributeGroup ref="SourceAttributeGroup"/>
		<xs:attributeGroup ref="ExchangeSourceAttributeGroup"/>
	</xs:complexType>
	<!-- END ExchangeSourceType -->
	<xs:complexType name="ExchangePOSType">
