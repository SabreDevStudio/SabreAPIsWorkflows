			<xs:annotation>
				<xs:documentation xml:lang="en">Base fare amount</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BaseFareCurrency" type="AlphaLength3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Base fare currency</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FareCalcCurrency" type="AlphaLength3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Fare calc currency</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ValidatingCarrier" type="CarrierCode" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Validating carrier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ROE" type="xs:double" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Rate of Exchange override (note: doesn't need to be specified if FareCalc currency and BaseFare currency is the same).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<!-- END ExchangeFareAttributeGroup -->
	<xs:complexType name="ExchangeFareType">
		<xs:attributeGroup ref="ExchangeFareAttributeGroup"/>
	</xs:complexType>
	<!-- END ExchangeFareType -->
	<xs:attributeGroup name="ExchangeSourceAttributeGroup">
		<xs:attribute name="AgentCity" type="AlphaLength3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Agent city</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MainTravelAgencyPCC" type="StringLength1to16" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Main travel agency PCC</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Carrier" type="CarrierCode" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Carrier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="HostCarrier" type="CarrierCode" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">PCC Host Carrier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ETicketCapable" type="xs:boolean" use="optional" default="false">
