    <xsd:complexType name="APISRequest.PNRB">
        <xsd:annotation>
            <xsd:documentation>Max line lengths

                * generic DOCS/DOCO - 108 chars
                after the first slash.
                Alphas, numerics, slashses and spaces are
                allowed.
                *segment specific DOCS/DOCO - 91 chars after the first
                slash
                * DOCA is variable. It is the same as DOCS and
                DOCO unless associated to more than one name. There is
                one less char allowed for
                each name to which the item is
                associated.
                * the AFAX DOCA (4DOCA) has a max length of 127 chars
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="DOCAEntry" type="DOCAEntry.PNRB">
                <xsd:annotation>
                    <xsd:documentation>Code indicating the type of SSR that was sent.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DOCOEntry" type="DOCOEntry.PNRB">
                <xsd:annotation>
                    <xsd:documentation>Code indicating the type of SSR that was sent.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DOCSEntry" type="DOCSEntry.PNRB"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="AccountingLine.PNRB">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="TypeIndicator" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Indicator of the accounting line, e.g. AC.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="FareApplication" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Fare Application indicates whom Form of Payment is applied to. Possible values:<br/>
			            ONE - a specific passenger<br/>
			            ALL - all the passengers in the PNR<br/>
			            PER - per person
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="FormOfPaymentCode" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Possible options: CK - check, CA - cash, CH - check or cash (trams only), CC - credit card, CX - credit card (printed with masking).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="LinkCode" type="CommonString"/>
            <xsd:element minOccurs="0" name="AccountingVendorCode" type="CommonString"/>
            <xsd:element minOccurs="0" name="ChargeCategoryCoded" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Charge category code, e.g. TKT.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AirlineDesignator" type="CommonString"/>
            <xsd:element minOccurs="0" name="DocumentNumber" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Number used in conjunction with Charge Category Code. Document Number must contain up to 7 digits.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CommissionPercentage" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Commission percentage in range 0-99.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CommissionAmount" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Commission amount in range 0-9,999,999.99.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="BaseFare" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Base fare amount in range 0-9,999,999.99.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="BasePercent" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Base fare percentage in range 0-99.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TaxPercentage" type="CommonString"/>
            <xsd:element minOccurs="0" name="TaxAmount" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Tax amount in range 0-9,999,999.99.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TaxSurchargeCode2" type="CommonString"/>
            <xsd:element minOccurs="0" name="GSTCode" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Goods and Services Tax or Value-Added Tax code: A, B, C or D.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="GSTAmount" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Goods and Services Tax or Value-Added Tax amount in range 0-9,999,999.99.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="GSTPercent" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Goods and Services Tax or Value-Added Tax percentage in range 0-99.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="QSTCode" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Quebec Sales Tax code: A, B, C or D.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="QSTAmount" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Quebec Sales Tax amount in range 0-9,999,999.99.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="QSTPercent" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Quebec Sales Tax percentage in range 0-99.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CreditCardNumber" type="PaymentCardNumber">
                <xsd:annotation>
                    <xsd:documentation>Credit Card Number containing at most 16 digits.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CreditCardCode" type="PaymentCardCode">
                <xsd:annotation>
                    <xsd:documentation>Two-letter code of Credit Card provider.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PassengerName" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Passenger used in Form of Payment association (if Fare Application is ONE). Entry must follow the pattern: [Name Number][Surname] [Given name or initial]</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="NumberOfConjunctedDocuments" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Number of documents the Accounting Line applies to. Amount in range 1-9</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="NumberOfCoupons" type="CommonString"/>
            <xsd:element minOccurs="0" name="OriginalTicketNumber" type="CommonString"/>
            <xsd:element minOccurs="0" name="OriginalDateOfIssue" type="CommonString"/>
            <xsd:element minOccurs="0" name="OriginalPlaceOfIssue" type="CommonString"/>
            <xsd:element minOccurs="0" name="FullPartialExchangeIndicator" type="CommonString"/>
            <xsd:element minOccurs="0" name="OriginalInvoice" type="CommonString"/>
            <xsd:element minOccurs="0" name="TarriffBasis" type="CommonString"/>
            <xsd:element minOccurs="0" name="FreeFormText" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Optional text which can contain up to 29 characters.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CurrencyCode" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Three-letter currency code.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SegmentType" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Type of segment the Accounting Line is associated to, e.g. RAL.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SegmentNumber" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>Number of segment the Accounting Line is associated to.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="UniqueIdentifier.PNRB"/>
        <xsd:attribute name="index" type="Numeric0to99999"/>
        <xsd:attribute name="op" type="OperationType.PNRB">
            <xsd:annotation>
                <xsd:documentation>C = Create, U = Update, D = Delete</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
		<xsd:attribute name="elementId" type="AssociationMatrixID.PNRB" use="optional"/>
    </xsd:complexType>
