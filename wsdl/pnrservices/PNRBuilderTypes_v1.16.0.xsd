    <xsd:complexType name="HistoryTransaction.PNRB">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Passengers" type="Passengers.PNRB"/>
            <xsd:element minOccurs="0" name="Segments" type="SegmentType.PNRB"/>
        </xsd:sequence>
        <xsd:attribute name="historySequence" type="CommonString"/>
        <xsd:attribute name="historyDateTime" type="xsd:dateTime"/>
    </xsd:complexType>
    <xsd:complexType name="ItineraryPricing.PNRB">
        <xsd:sequence>
            <xsd:element maxOccurs="99" minOccurs="0" name="FuturePriceInfo" type="FuturePriceInfo.PNRB">
                <xsd:annotation>
                    <xsd:documentation>Future price information.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="PricedItinerary" type="PricedItinerary.PNRB">
                <xsd:annotation>
                    <xsd:documentation>Priced Itinerary information.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="numberInParty" type="Numeric0to99999">
