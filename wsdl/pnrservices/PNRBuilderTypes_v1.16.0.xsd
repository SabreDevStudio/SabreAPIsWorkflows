        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="GenericSpecialRequest" type="GenericSpecialRequest.PNRB"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="OTHSFOPMessage" type="OTHSFOPMessage.PNRB"/>
