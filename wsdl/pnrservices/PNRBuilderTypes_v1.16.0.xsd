            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="AssociationMatrixID.PNRB">
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
	<xsd:complexType name="AssociationMatrix.PNRB">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="Parent">
				<xsd:complexType>
					<xsd:attribute name="ref" type="AssociationMatrixID.PNRB" use="optional"/>
					<xsd:attribute name="newRef" type="xsd:string" use="optional"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Child" maxOccurs="unbounded">
				<xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0"  maxOccurs="unbounded" name="AssociationRule" >
                            <xsd:annotation>
                                <xsd:documentation>
                                    Populate Association rule details from PDS.
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:attribute name="name" type="xsd:string"/>
                                <xsd:attribute name="value" type="xsd:string"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="ref" type="AssociationMatrixID.PNRB" use="optional"/>
					<xsd:attribute name="newRef" type="xsd:string" use="optional"/>
					<xsd:attribute name="key" type="xsd:string" use="optional"/>
					<xsd:attribute name="value" type="xsd:string" use="optional"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="elementId" type="AssociationMatrixID.PNRB"/>
	</xsd:complexType>
	<xsd:complexType name="AssociationMatrixUpdate.PNRB">
		<xsd:complexContent>
			<xsd:extension base="AssociationMatrix.PNRB">
				<xsd:attribute name="op" type="OperationType.PNRB">
					<xsd:annotation>
						<xsd:documentation>C = Create, U = Update, D = Delete</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AssociationMatrices.PNRB">
		<xsd:sequence>
			<xsd:element name="AssociationMatrix" type="AssociationMatrix.PNRB" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AssociationUpdate.PNRB">
		<xsd:sequence>
			<xsd:element name="Parent" type="AssociationParent" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Child" type="AssociationChild" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="op" type="AssociationOperationType">
			<xsd:annotation>
				<xsd:documentation>CREATE, UPDATE, DELETE, KEEP</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:simpleType name="AssociationParentType">
		<xsd:annotation>
			<xsd:documentation>- NameNumber e.g. 1.1, only type currently supported</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NameNumber"/>
			<xsd:enumeration value="NameSeqId"/>
			<xsd:enumeration value="FOP"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AssociationParentReferenceType">
		<xsd:annotation>
			<xsd:documentation>- Index : the index/sequence number for the item being associated to</xsd:documentation>
			<xsd:documentation>- UkyId : the LREC Uky of the item being associated to</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Index"/>
			<xsd:enumeration value="UkyId"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="AssociationParent">
		<xsd:attribute name="Reference" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Key value of the parent item being associated to</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="Type" type="AssociationParentType">
			<xsd:annotation>
				<xsd:documentation>Type of value passed in primaryRef</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ReferenceType" type="AssociationParentReferenceType">
			<xsd:annotation>
				<xsd:documentation>Type of value passed in Reference, which is the item to be associated as parent</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:simpleType name="AssociationChildReferenceType">
		<xsd:annotation>
			<xsd:documentation>- Index : the index/sequence number for the item being associated</xsd:documentation>
			<xsd:documentation>- UkyId : the LREC Uky of the item being associated</xsd:documentation>
			<xsd:documentation>- New : the item being associated is new, and so the reference needs to be generated
			</xsd:documentation>
			<xsd:documentation>- Type : the association action should be applied to all items of the given type
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Index"/>
			<xsd:enumeration value="UkyId"/>
			<xsd:enumeration value="New"/>
			<xsd:enumeration value="Type"/>
			<xsd:enumeration value="Static"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AssociationChildElementType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Remark"/>
			<xsd:enumeration value="PhoneNumber"/>
			<xsd:enumeration value="Ticket"/>
			<xsd:enumeration value="AccountingLine"/>
			<xsd:enumeration value="FOP"/>
			<xsd:enumeration value="FopUsageType"/>
			<xsd:enumeration value="FopTripType"/>
			<xsd:enumeration value="AccountingField"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="AssociationChild">
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="RemarkType" type="RemarkType.PNRB" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Only included when Child/@Type=Remark and Child/@ReferenceType=Type
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attribute name="Reference" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Key value of the child item being associated</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="TempReference" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Key value of the child item being associated which will be updated with reference
					value.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ReferenceType" type="AssociationChildReferenceType">
			<xsd:annotation>
				<xsd:documentation>Type of value passed in Reference, which is the item to be associated to the parent
					item
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="Type" type="AssociationChildElementType">
			<xsd:annotation>
				<xsd:documentation>Type of data, optional if Op equals U</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
    <xsd:simpleType name="AssociationOperationType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ADD"/>
            <xsd:enumeration value="UPDATE"/>
            <xsd:enumeration value="DELETE"/>
            <xsd:enumeration value="KEEP"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="PshUpdate.PNRB">
