    </xsd:complexType>
    <xsd:complexType name="PNRUpdateReservationRQ">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Reservation" type="Reservation.PNRB"/>
            <xsd:element minOccurs="0" name="POS" type="POS_Type.PNRB"/>
            <xsd:element minOccurs="0" name="ReservationEmail" type="ReservationEmail.PNRB"/>
            <xsd:element minOccurs="0" name="RecordLocator" type="CommonString"/>
