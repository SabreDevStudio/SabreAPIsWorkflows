    <xsd:complexType name="FeeDetails.Misc">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Base" type="Amount">
                <xsd:annotation>
                    <xsd:documentation>The base fee amount associated to the item or service which does not include
                        taxes. Mapped to: SR05_C1_BAMT
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Equiv" type="Amount">
                <xsd:annotation>
                    <xsd:documentation>The equivalent or converted fee amount associated to the item or service which
                        does not include taxes. Mapped to: SR05_C1_BAMT
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TotalTax" type="Amount">
                <xsd:annotation>
                    <xsd:documentation>The total fee taxes associated to the item or service purchased. Mapped to:
                        SR05_C1_TAMT
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Total" type="Amount">
                <xsd:annotation>
                    <xsd:documentation>The total fee amount associated to the service including taxes. Mapped to:
                        SR05_C1_FAMT
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="EffectiveDate" type="xsd:date">
                <xsd:annotation>
                    <xsd:documentation>The date the fee and amounts will be effective or can be used for purchase.
                        Mapped to: SR05_C1_EFFD
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="code" type="StringLength1to3" use="required">
            <xsd:annotation>
                <xsd:documentation>PRAS code associated to the service or item. Mapped to: SR05_C1_PRAS
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="description" type="StringLength1to21">
            <xsd:annotation>
                <xsd:documentation>Description of the purchased item or service. Mapped to: SR05_C1_DESC
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="quantity" type="xsd:int">
            <xsd:annotation>
                <xsd:documentation>The number of items purchased per PRAS code. Mapped to: SR05_C1_IQTY
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:simpleType name="StringLength1to3">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="3"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="StringLength1to21">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="21"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="StringLength1to20">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="20"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="StringLength1to10">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="10"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="AlphaNumericStringLength2to3">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9a-zA-Z]{2,3}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="Tax.Misc">
        <xsd:sequence>
            <xsd:element name="Amount" type="Amount"/>
        </xsd:sequence>
        <xsd:attribute default="false" name="exempt" type="xsd:boolean">
            <xsd:annotation>
                <xsd:documentation>An indicator that a tax has been exempted. Mapped to:</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="code" type="AlphaNumericStringLength2to3" use="required">
            <xsd:annotation>
                <xsd:documentation>A code that applies to the tax (e.g, US, ZP). Mapped to: SR05_C2_TCOD
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="PersonName">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Prefix" type="StringLength1to16">
                <xsd:annotation>
                    <xsd:documentation>Salutation of honorific. (e.g., Mr. Mrs., Ms., Miss, Dr.)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Given" type="ProperName">
                <xsd:annotation>
                    <xsd:documentation>Given name, first name or names</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Middle" type="ProperName">
                <xsd:annotation>
                    <xsd:documentation>The middle name of the person name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Surname" type="ProperName">
                <xsd:annotation>
                    <xsd:documentation>Family name aslo known as last name.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Suffix" type="StringLength1to16">
                <xsd:annotation>
                    <xsd:documentation>Hold various name suffixes and letters (e.g. Jr., Sr., III, Ret., Esq.).
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Type" type="PersonNameType"/>
            <xsd:element minOccurs="0" name="PreferredFirstName" type="ProperName">
                <xsd:annotation>
                    <xsd:documentation>Any preferred names for first name used instead of GivenName</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PreferredSurname" type="ProperName">
                <xsd:annotation>
                    <xsd:documentation>Any preferred names for last names used instead of surname</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="PersonNameType">
        <xsd:restriction base="StringLength0to32">
            <xsd:enumeration value="Former"/>
            <xsd:enumeration value="Nickname"/>
            <xsd:enumeration value="Alternate"/>
            <xsd:enumeration value="Maiden"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="StringLength0to32">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="0"/>
            <xsd:maxLength value="32"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="ClientContext">
        <xsd:sequence>
            <xsd:element name="Name" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>If context is set in GetReservation request - master PNR is retrieved
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ResultAction">
        <xsd:choice minOccurs="0">
            <xsd:element maxOccurs="1" minOccurs="0" name="QueuePlaceAction">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="1" name="QueuePlacement" type="QueuePlacement">
                            <xsd:annotation>
                                <xsd:documentation>Number and pit code of the queue, where pnr should be placed</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="1" name="EvaluateOnly" type="xsd:boolean"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="QueuePlacement">
        <xsd:sequence>
            <xsd:element minOccurs="1" name="QueueNumber">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:int">
                        <xsd:minInclusive value="0"/>
                        <xsd:maxInclusive value="511"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="CityCode" type="xsd:string"/>
            <xsd:element minOccurs="1" name="PitCode">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:int">
                        <xsd:minInclusive value="0"/>
                        <xsd:maxInclusive value="254"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="NativeLanguageValueType">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute name="lang" type="xsd:language"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:simpleType name="AddressType.PNRB">
        <xsd:annotation>
            <xsd:documentation>Address Line Type - "A" - Address; "C" - City-State-Country;
				 "N" - Name;  "Z" - ZipCode; "B" - Business; "O" - Default-Other
			</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="N"/>
            <xsd:enumeration value="A"/>
            <xsd:enumeration value="C"/>
            <xsd:enumeration value="Z"/>
            <xsd:enumeration value="B"/>
            <xsd:enumeration value="O"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="AirPosType.PNRB">
        <xsd:annotation>
            <xsd:documentation>OAC POS details for segments
			</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="CrsCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>OAC POS Crs code</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="IataNumber" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>OAC POS IATA number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AgencyCityCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>OAC POS Agency city code</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CountryCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>OAC POS Booking country code</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DutyCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>OAC POS Duty code</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="OACAccountingCityCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>OAC Accounting City code</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="OACAccountingCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>OAC Accounting code</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="FqtvUpgradeRequests">
        <xsd:annotation>
            <xsd:documentation>List of Fqtv Upgrade requests associated to
				passenger and air.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="FqtvUpgradeRequest" type="FqtvUpgradeRequest"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="FqtvUpgradeRequest">
        <xsd:annotation>
            <xsd:documentation>Flight and frequent traveler upgrade request
				details
			</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="FlightBasicData">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="StatusCode" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="OriginalRequestTimestamp" type="xsd:dateTime"/>
                    <xsd:element minOccurs="0" name="LocalDepartureDate" type="xsd:dateTime"/>
                    <xsd:element minOccurs="0" name="TierLevel" type="xsd:int"/>
                    <xsd:element minOccurs="0" name="RequestedUpgradeClass" type="CommonString"/>
                    <xsd:element minOccurs="0" name="CompanionData" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="FrequentTravelerAirline" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="FrequentTravelerNumber" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="FreeText" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="SSRPreference" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="SeatPreference" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PassengerName" type="xsd:string"/>
                </xsd:sequence>
                <xsd:attribute name="UpgradeType" type="xsd:string"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="HeaderUpdateItem.PNRB">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="0" name="OpenSystemReferenceId" type="OpenSystemReferenceId.PNRB"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="OpenSystemReferenceId.PNRB">
        <xsd:restriction base="xsd:string">
            <xsd:length value="16"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="ODDataType">
        <xsd:annotation>
            <xsd:documentation>Bid Price and Fare Reference data - Pnr footprint</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="OriginalFare" type="xsd:int">
                <xsd:annotation>
                    <xsd:documentation>Original Fare value retrieved from O and D</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="EffectiveBidPrice" type="xsd:int">
                <xsd:annotation>
                    <xsd:documentation>Effective bid price</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AdjustedFare" type="xsd:int">
                <xsd:annotation>
                    <xsd:documentation>Adjusted final fare used for evaluation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AdjustedBidPrice" type="xsd:int">
                <xsd:annotation>
                    <xsd:documentation>Adjusted bid price</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="FareClass" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Orign and Destination Fare Class</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="LowestAvailFareClass" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Lowest Orign and Destination class available</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PointOfCommencement" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Point of Commencement</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CustomerScore" type="Numeric0to99999">
                <xsd:annotation>
                    <xsd:documentation>Customer Value Score</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="FrequentFlyer" type="ODFrequentFlyerType">
                <xsd:annotation>
                    <xsd:documentation>Frequent Flyer</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Market" type="ODMarketType">
                <xsd:annotation>
                    <xsd:documentation>Financial Orign and Destination with Level</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TripMarket" type="ODMarketType">
                <xsd:annotation>
                    <xsd:documentation>Trip Orign and Destination with Level</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="RetrievedFareMarket" type="ODMarketType">
                <xsd:annotation>
                    <xsd:documentation>Retrieved Fare Orign and Destination with Level</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="FareTypeIndicators" type="ODFareTypeIndicatorsType">
                <xsd:annotation>
                    <xsd:documentation>Fare Type Indicators</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="EvaluationTypeIndicators" type="ODEvaluationTypeIndicatorsType">
                <xsd:annotation>
                    <xsd:documentation>Evaluation Type Indicators</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ExceptionTypeIndicators" type="ODExceptionTypeIndicatorsType">
                <xsd:annotation>
                    <xsd:documentation>Exception Type Indicators</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AdditionalFlags" type="xsd:unsignedLong">
                <xsd:annotation>
                    <xsd:documentation>Additional Flags - undefined</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AdditionalData" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Additional Data - undefined</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="uniqueId" type="xsd:string" use="optional"/>
    </xsd:complexType>
    <xsd:complexType name="ODFareTypeIndicatorsType">
        <xsd:attribute default="false" name="regular" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="sponsor" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="default" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="sponsorDefault" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="hostDefault" type="xsd:boolean" use="optional"/>
    </xsd:complexType>
    <xsd:complexType name="ODEvaluationTypeIndicatorsType">
        <xsd:attribute default="false" name="default" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="sumOfLocals" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="trueOD" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="longestSegmentFare" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="dominantSegmentFare" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="local" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="financial" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="physical" type="xsd:boolean" use="optional"/>
    </xsd:complexType>
    <xsd:complexType name="ODExceptionTypeIndicatorsType">
        <xsd:attribute default="false" name="forcedSell" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="schedChg" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="forcedPartialCancel" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="ttyRejectAgent" type="xsd:boolean" use="optional"/>
        <xsd:attribute default="false" name="ttyTransaction" type="xsd:boolean" use="optional"/>
    </xsd:complexType>
    <xsd:complexType name="ODMarketType">
        <xsd:attribute name="destination" type="CommonString" use="optional"/>
        <xsd:attribute name="destinationLevel" type="Numeric0to99999" use="optional"/>
        <xsd:attribute name="origin" type="CommonString" use="optional"/>
        <xsd:attribute name="originLevel" type="Numeric0to99999" use="optional"/>
    </xsd:complexType>
    <xsd:complexType name="ODFrequentFlyerType">
        <xsd:attribute name="tierLevel" type="Numeric0to99999" use="optional"/>
        <xsd:attribute name="supplierCode" type="CommonString" use="optional"/>
    </xsd:complexType>
    <xsd:simpleType name="BrandIDType" >
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="2"/>
            <xsd:maxLength value="10"/>
        </xsd:restriction>
    </xsd:simpleType>

</xsd:schema>
