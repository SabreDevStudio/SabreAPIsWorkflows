    </xsd:complexType>
    <xsd:complexType name="PNRUpdateReservationRS">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Reservation" type="Reservation.PNRB"/>
            <xsd:element minOccurs="0" name="POS" type="POS_Type.PNRB"/>
            <xsd:element minOccurs="0" name="RecordLocator" type="CommonString"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="POS_Type.PNRB">
        <xsd:annotation>
            <xsd:documentation>Point of Sale (POS) is the details identifying the party or connection channel making the
                request.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Source" type="SourceType.PNRB">
                <xsd:annotation>
                    <xsd:documentation>This holds details regarding the requestor. It may be repeated to also
                        accommodate the delivery systems.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="Passenger.PNRB">
        <xsd:sequence>
            <xsd:element name="LastName" type="CommonString">
                <xsd:annotation>
                    <xsd:documentation>LastName. Ex."WOOD"</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="OriginalLastName" type="NativeLanguageValueType"/>
            <xsd:element minOccurs="0" name="FirstName" type="CommonString"/>
            <xsd:element minOccurs="0" name="OriginalFirstName" type="NativeLanguageValueType"/>
            <xsd:element minOccurs="0" name="Title" type="CommonString"/>
            <xsd:element maxOccurs="99" minOccurs="0" name="FrequentFlyer" type="FrequentFlyer.PNRB"/>
            <xsd:element maxOccurs="99" minOccurs="0" name="EmailAddress" type="EmailAddress.PNRB">
                <xsd:annotation>
                    <xsd:documentation>Email address added to the PNR. Ex."ANNE@MAIL.COM"</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Gender" type="CommonString"/>
            <xsd:element minOccurs="0" name="DateOfBirth" type="xsd:date"/>
            <xsd:element minOccurs="0" name="Profiles" type="Profiles.PNRB"/>
            <xsd:element minOccurs="0" name="SpecialRequests" type="NameAssociatedSpecialRequests.PNRB"/>
            <xsd:element minOccurs="0" name="Seats" type="Seats.PNRB"/>
            <xsd:element minOccurs="0" name="PreReservedSeats" type="PreReservedSeats.PNRB"/>
            <xsd:element minOccurs="0" name="AccountingLines" type="AccountingLines.PNRB"/>
            <xsd:element minOccurs="0" name="PassengerProfileID" type="xsd:string"/>
            <xsd:element minOccurs="0" name="AncillaryServices">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="AncillaryService" type="AncillaryServices.PNRB"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="OSI">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="OSIMessage" type="OSIType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="Remarks" type="Remarks.PNRB"/>
            <xsd:element minOccurs="0" name="PhoneNumbers" type="PhoneNumbers.PNRB"/>
            <xsd:element minOccurs="0" name="TicketingInfo" type="TicketingInfo.PNRB"/>
            <xsd:element minOccurs="0" name="FqtvUpgradeRequests" type="FqtvUpgradeRequests"/>
			<xsd:element minOccurs="0" name="OpenReservationElements" type="OpenReservationElementsType"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="UniqueIdentifier.PNRB"/>
        <xsd:attribute name="nameType" type="PassengerType.PNRB">
            <xsd:annotation>
                <xsd:documentation>I = Infant, S = Standard</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="passengerType" type="CommonString"/>
        <xsd:attribute name="referenceNumber" type="CommonString"/>
        <xsd:attribute name="nameId" type="CommonString">
            <xsd:annotation>
                <xsd:documentation>Name id. Ex."02.02"</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="nameAssocId" type="CommonString"/>
        <xsd:attribute name="withInfant" type="xsd:boolean">
            <xsd:annotation>
                <xsd:documentation>"with infant" True/False indicator. Ex. "TRUE"</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="op" type="OperationType.PNRB">
            <xsd:annotation>
                <xsd:documentation>C = Create, U = Update, D = Delete</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="lang" type="CommonString"/>
		<xsd:attribute name="elementId" type="AssociationMatrixID.PNRB" use="optional"/>
    </xsd:complexType>
    <xsd:complexType name="Seats.PNRB">
