										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="Amount" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
													<xsd:annotation>
														<xsd:documentation>"Amount" is used to return the vehicle charge amount.</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="ConversionRate" minOccurs="0">
