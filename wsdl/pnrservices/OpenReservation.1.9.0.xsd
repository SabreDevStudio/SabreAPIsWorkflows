		<xsd:attribute name="linkCode" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>"linkCode" is used to return the link code</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="mergedSegmentInd" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>"mergedSegmentInd" is used to return the merged segment indicator associated with the particular itinerary segment if applicable.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
