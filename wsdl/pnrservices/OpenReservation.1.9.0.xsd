																	<xsd:documentation>Vehicle Upgrade field  indicates upgrade field provided by supplier  /UPG-@ECAR</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="Voucher" minOccurs="0">
													<xsd:complexType>
