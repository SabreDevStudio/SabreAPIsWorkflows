			<xsd:element name="TravellerAllocation" type="xsd:string" minOccurs="0" />
			<xsd:element name="ServicePrice" type="PriceType" minOccurs="0" />
			<xsd:element name="StealBoarding" type="xsd:string" minOccurs="0" />
		</xsd:sequence>
		<xsd:attributeGroup ref="SegmentCommonAttributes" />
		<xsd:attribute name="id" type="xsd:string" />
	</xsd:complexType>

	<xsd:complexType name="DetailType">
