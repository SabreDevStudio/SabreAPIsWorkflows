										<xsd:documentation>"code" is used to return the vehicle vendor code.</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
								<xsd:attribute name="companyShortName" type="xsd:string" use="optional">
									<xsd:annotation>
										<xsd:documentation>"companyShortName" is used to return the company name associated with the particular vehicle vendor code.</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="PricingElements" type="PricingElementsType" minOccurs="0"/>
