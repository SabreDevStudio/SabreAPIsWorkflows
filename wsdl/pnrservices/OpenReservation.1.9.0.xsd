									<xsd:element type="xsd:string" name="CoachNumber" minOccurs="0"/>
									<xsd:element name="PlacementNumber" minOccurs="0" maxOccurs="unbounded">
										<xsd:complexType>
											<xsd:simpleContent>
												<xsd:extension base="xsd:string">
