											<xsd:attribute name="rateCode" type="xsd:string" use="optional">
												<xsd:annotation>
													<xsd:documentation>Rate item Rate Code  /RC-STDR</xsd:documentation>
												</xsd:annotation>
											</xsd:attribute>
											<xsd:attribute name="stmRatePlan" type="xsd:string" use="optional">
												<xsd:annotation>
													<xsd:documentation>Rate item rate plan  /RG-USD52.72 UNL DY XH8.79 UNL</xsd:documentation>
												</xsd:annotation>
											</xsd:attribute>
										</xsd:complexType>
