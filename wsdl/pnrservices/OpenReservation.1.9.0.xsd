                        			</xsd:extension>
                        		</xsd:simpleContent>
        			</xsd:complexType>
			</xsd:element>
			<xsd:element name="PassengerReferences" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="PassengerRef" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:attribute name="id"/>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Accommodation" type="xsd:string" minOccurs="0" />
