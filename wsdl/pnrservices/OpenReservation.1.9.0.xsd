			<xsd:element name="Country" type="CodeDescriptionType" minOccurs="0"/>
			<xsd:element name="CountryCodes" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Code" maxOccurs="unbounded">
