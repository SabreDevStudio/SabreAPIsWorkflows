                    <xsd:documentation source="Description">Additional information about seat occupation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Description" type="ota2-dep:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">Free text additional information</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Seat_Location" type="imap-0400:Seat_Location"/>
    <xsd:element name="Location" type="imap-0400:Seat_Location"/>
    
    <xsd:complexType name="Seat_Location">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Seat_Location</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Detail" type="imap-0400:Enum_SeatLocationType">
                <xsd:annotation>
                    <xsd:documentation source="Description">Additional information about seat occupation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Description" type="ota2-dep:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">Free text additional information</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
