                    <xsd:documentation source="Description">Column characteristcis applicable to whole column</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="9" minOccurs="0" ref="imap-0400:Price">
                <xsd:annotation>
                    <xsd:documentation source="Description">Price and taxes applicable to seats in this column</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Seat_Occupation" type="imap-0400:Seat_Occupation"/>
    <xsd:element name="Occupation" type="imap-0400:Seat_Occupation"/>
    
    <xsd:simpleType name="Seat_Occupation_Simple">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Seat_Occupation</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="imap-0400:Enum_SeatOccupation"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="Seat_Occupation_Simple_List">
        <xsd:list itemType="imap-0400:Seat_Occupation_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="Seat_Occupation">
