                    <xsd:documentation source="Description">Additional information about seat occupation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Description" type="ota2-dep:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">Free text for additional information on the occupation type</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Seat_Facilities" type="imap-0400:Seat_Facilities"/>
    <xsd:element name="Facilities" type="imap-0400:Seat_Facilities"/>
    
