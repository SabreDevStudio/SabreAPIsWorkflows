                    <xsd:documentation source="Description">Seat Column A-Z</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="2" minOccurs="0"
                name="Characteristics" type="imap-0400:Enum_ColumnDescription">
