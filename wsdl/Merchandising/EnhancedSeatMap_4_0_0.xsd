        <xsd:annotation>
            <xsd:documentation source="Description">PNR record locator</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">PNRLOC</otm:OTA2Entity>
