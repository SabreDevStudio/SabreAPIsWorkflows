                <otm:OTA2Entity type="Simple">SeatLetter</otm:OTA2Entity>
                <otm:Example context="SeatMap">A</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Z]"/>
        </xsd:restriction>
