    <xsd:complexType name="CustomerLoyalty">
        <xsd:annotation>
            <xsd:documentation source="Description">Travellers Loyalty Membership Number</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://services.sabre.com/STL/v01}MembershipID</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">CustomerLoyalty</otm:OTA2Entity>
                <otm:Equivalent context="SeatMap">CustomerLoyality.OTA</otm:Equivalent>
                <otm:Example context="SeatMap">WWA345876</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="stl2:MembershipID">
                <xsd:attribute name="carrier"
                    type="ota2-dep:Code_IATA_AirlineID" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Loyalty program owning carrier</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="SeatMap">YY</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="program"
                    type="ota2-dep:Name_Proper" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Primary Membership program</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://services.sabre.com/STL/v01}ProperName</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="SeatMap">program</otm:Equivalent>
                            <otm:Example context="SeatMap">WWA-Loyality</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="partnerProgram"
                    type="ota2-dep:Name_Proper" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Partner or Alliance</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://services.sabre.com/STL/v01}ProperName</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="SeatMap">partnerProgram</otm:Equivalent>
                            <otm:Example context="SeatMap">AroundTheWorld</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="level" type="ota2-dep:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Frequent Flier Tier Number stored in PNR</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://services.sabre.com/STL/v01}StringLength1to64</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="SeatMap">level</otm:Equivalent>
                            <otm:Example context="SeatMap">3</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="notes" type="ota2-dep:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Free text</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://services.sabre.com/STL/v01}Text</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="SeatMap">notes</otm:Equivalent>
                            <otm:Example context="SeatMap">FreeText</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="tag" type="ota2-dep:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Frequent Flier Tag stored in PNR</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="SeatMap">GLD</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="profileTier"
                    type="ota2-dep:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Frequent Flier Tier stored in Profiles</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="SeatMap">3</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="Flight_Identifier">
        <xsd:annotation>
            <xsd:documentation source="Description">Flight Number</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Flight_Identifier</otm:OTA2Entity>
                <otm:Equivalent context="SeatMap">Flight.Identifier</otm:Equivalent>
                <otm:Example context="SeatMap">234</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="stl2:FlightNumber">
                <xsd:attribute name="carrier" type="stl2:Code_Airline" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">By default, the carrier is assumed from the transaction context</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="SeatMap">carrier</otm:Equivalent>
                            <otm:Example context="SeatMap">WWA</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="suffix"
                    type="stl2:FlightNumber_Suffix" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Flight Identifier Suffix is used by operations to differentiate flights with same flight number operating on the same date</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="SeatMap">A</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="Code">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Code</otm:OTA2Entity>
                <otm:Example context="SeatMap">ABC</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="stl2:String_AlphaNumeric">
                <xsd:attribute name="URI" type="xsd:anyURI" use="optional">
                    <xsd:annotation>
                        <xsd:appinfo>
                            <otm:Example context="SeatMap">http://example.com</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="context"
                    type="ota2-dep:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:appinfo>
                            <otm:Example context="SeatMap">OTA</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="Amount">
        <xsd:annotation>
            <xsd:documentation source="Description">A numeric monetary value.</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type:
				{http://services.sabre.com/STL/v01}_amount</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Amount</otm:OTA2Entity>
                <otm:Example context="SeatMap">25.00</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:decimal">
                <xsd:attribute name="currencyCode"
                    type="ota2-dep:Code_ISO_Currency" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">ISO Currency code</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type:
					{http://services.sabre.com/STL/v01}CurrencyCodeList</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="SeatMap">USD</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="currency"
                    type="ota2-dep:Name_Proper" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Currency name</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type:
					{http://services.sabre.com/STL/v01}ProperName</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="SeatMap">Euro</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="decimalPlaces"
                    type="xsd:positiveInteger" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Number of decimal places used with this currency.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="SeatMap">2</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="POS_Location">
        <xsd:annotation>
            <xsd:documentation source="Description">A location for an airline or agency agent. Can be used
				to describe the actual, pseudo or home location.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">POS_Location</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2-dep:Empty">
                <xsd:attribute name="country" type="stl2:Code_Country" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">3 character country code agency is located in</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="province"
                    type="stl2:Code_StateProv" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">State or province</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="city" type="stl2:Code_City" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">City</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="number"
                    type="stl2:Number_AgencyOrStation" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">agent station number or agency iata number</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="SeatMap">1112223</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="country2"
                    type="stl2:Code_CountryLegacy" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">2 character country code for agency</xsd:documentation>
                        <xsd:documentation source="Implementer">It is preferred to use the 3 character country code. This is provided for those cases where the 3 character country code is not supported and the  2 character country code is required.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="FlightDate">
        <xsd:annotation>
            <xsd:documentation source="Description">Departure date this segment</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">FlightDate</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:date">
                <xsd:attribute name="localTime" type="xsd:time" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Departure time this segment in local time</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="ClientContext">
        <xsd:annotation>
            <xsd:documentation source="Description">Client context details.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">ClientContext</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2-dep:Empty">
                <xsd:attribute name="clientType"
                    type="ota2-dep:String_Short" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Client context.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:element name="Flight_Segment" type="imap-0400:Flight_Segment"/>
    <xsd:element name="Flight" type="imap-0400:Flight_Segment"/>
    
    <xsd:complexType name="Flight_Segment">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Flight_Segment</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="DepartureDate" type="imap-0400:FlightDate">
                <xsd:annotation>
                    <xsd:documentation source="Description">Departure date this segment</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Operating" type="imap-0400:Flight_Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">Operating Airline</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="10" minOccurs="1" name="Marketing" type="imap-0400:Flight_Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">Marketing Airline</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ArrivalDate" type="imap-0400:FlightDate">
                <xsd:annotation>
                    <xsd:documentation source="Description">Arrival date this segment</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AircraftConfigNumber" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">flight aircraft configuration</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Status" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">status of flight like open, close, boarding, etc.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="destination"
            type="ota2-dep:Code_IATA_Airport" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">IATA airport code</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">CDG</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="origin" type="ota2-dep:Code_IATA_Airport" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">IATA airport code</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">DFW</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Flight_Segment_Journey_data" type="imap-0400:Flight_Segment_Journey_data"/>
    <xsd:element name="FlightJourneyData" type="imap-0400:Flight_Segment_Journey_data"/>
    
    <xsd:complexType name="Flight_Segment_Journey_data">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Flight_Segment_Journey_data</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="CnxxIndicator" type="imap-0400:ChoiceType">
                <xsd:annotation>
                    <xsd:documentation source="Description">If Y, there are one or more connections.</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">CnxxIndicator</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="ClassOfService" type="stl2:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">Class of service.</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">ClassOfService</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ActionCode" type="stl2:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">Reservation system action code.Example: HK
                               ACCEPTED VALUES: 2 character reservation code.Sabre will only pass segments
                                                                                                    with SS and HK status as journey control data</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">ActionCode</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Cabin_WingRowDetails" type="imap-0400:Cabin_WingRowDetails"/>
    <xsd:element name="Wing" type="imap-0400:Cabin_WingRowDetails"/>
    
    <xsd:complexType name="Cabin_WingRowDetails">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Cabin_WingRowDetails</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="firstRow" type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">firs trow overwing</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">16</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="lastRow" type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">last row overwing</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">19</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Cabin_FacilitiesDetails" type="imap-0400:Cabin_FacilitiesDetails"/>
    <xsd:element name="Facility" type="imap-0400:Cabin_FacilitiesDetails"/>
    
    <xsd:complexType name="Cabin_FacilitiesDetails">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Cabin_FacilitiesDetails</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Characteristics" type="imap-0400:Enum_CabinFacilitiesType">
                <xsd:annotation>
                    <xsd:documentation source="Description">Padis  9976</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Location" type="imap-0400:Enum_CabinFacilitiesLocation">
                <xsd:annotation>
                    <xsd:documentation source="Description">Padis 3227</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Column" type="imap-0400:SeatLetter">
                <xsd:annotation>
                    <xsd:documentation source="Description">Seat Column A-Z</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="SeatMap_FareAvailabilityQualifiers" type="imap-0400:SeatMap_FareAvailabilityQualifiers"/>
    <xsd:element name="FareAvailQualifiers" type="imap-0400:SeatMap_FareAvailabilityQualifiers"/>
    
    <xsd:complexType name="SeatMap_FareAvailabilityQualifiers">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">SeatMap_FareAvailabilityQualifiers</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="TravellerID" type="ota2-dep:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">Traveler Profile Identifier.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="GivenName" type="ota2-dep:Name_Proper">
                <xsd:annotation>
                    <xsd:documentation source="Description">Firt or Given Name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Surname" type="ota2-dep:Name_Proper">
                <xsd:annotation>
                    <xsd:documentation source="Description">Last or Surname</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="9" minOccurs="0" name="Loyalty" type="imap-0400:CustomerLoyalty">
                <xsd:annotation>
                    <xsd:documentation source="Description">Loyalty program information in event it affects seatmap selection</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="9" minOccurs="0" name="SSR" type="ota2-dep:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">SSR Details</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="imap-0400:SeatReq"/>
            <xsd:element minOccurs="0" name="TravellerCharacteristics" type="ota2-dep:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">TODO - Use Open Enumeration from OTA - Traveller Characteristics or restrictions, pet allergies so no near a pet etc</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TicketNumber" type="stl2:Ticket_Number"/>
            <xsd:element minOccurs="0" ref="imap-0400:FareBreakCriteria">
                <xsd:annotation>
                    <xsd:documentation source="Description">Fare break criteria</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="quantity" type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies the number of travelers of a given passenger type (e.g., Adult, Child)</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: PassengerTypeQuantityType</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="SeatMap">PassengerTypeQuantity</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RBD" type="stl2:RBD" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Reservation Booking Designator - actual booking class code or F/J/Y for unbooked travelers.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">Y</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="birthDate" type="xsd:date" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Traveler's date of birth. Can be used for minumum age requirements for certain seat selection</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}date</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="SeatMap">BirthDate</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PNR_Loc" type="imap-0400:PNRLOC" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">PNR record Locator of passenger or group</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">ABCDEF</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="fareBasisCode" type="ota2-dep:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Fare Basis Code for the filed fare.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="tourCode" type="ota2-dep:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Fare tour code</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">TOUR123</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="passengerType" type="ota2-dep:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Pricing Passenger Type, for instance "GOV", "MIL", "ADT", "CNN"</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">ADT</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="fareComponentID" type="stl2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Fare component ID</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">1</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="accompaniedByInfantInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">If indicator is present and true the passenger is travelling with an infant that is not occupying a seat
Restrictions as to where the passenger may sit on the aircraft may apply due to exit row restrictions or the availbility of oxygen masks.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="FareAvailabilityQualifiers_FareBreakCriteria" type="imap-0400:FareAvailabilityQualifiers_FareBreakCriteria"/>
    <xsd:element name="FareBreakCriteria" type="imap-0400:FareAvailabilityQualifiers_FareBreakCriteria"/>
    
    <xsd:complexType name="FareAvailabilityQualifiers_FareBreakCriteria">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">FareAvailabilityQualifiers_FareBreakCriteria</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="fareComponentID" type="stl2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Fare Component Identifier</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">1</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="governingCarrier" type="stl2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Carrier code</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">EY</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="fareAmount" type="xsd:decimal" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Fare amount with decimal points</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">160.11</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="fareType" type="stl2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Fare type string</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">XPF</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="atpcoFareTariff"
            type="ota2-dep:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Aptco fare tariff</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">21</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="atpcoFareRule" type="stl2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Atpco fare rule</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">4U0</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FareIndicator" type="xsd:integer" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">fare indicator</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="fareTypeBitmap" type="stl2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">fare type bitmap</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">00</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="vendorCode" type="stl2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Vendor Code</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">ATP</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PrivateTariffIndicatorInd"
            type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">private tariff indicator</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="ColumnDetail" type="imap-0400:ColumnDetail"/>
    <xsd:element name="Column" type="imap-0400:ColumnDetail"/>
    
    <xsd:complexType name="ColumnDetail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">ColumnDetail</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Column" type="imap-0400:SeatLetter">
