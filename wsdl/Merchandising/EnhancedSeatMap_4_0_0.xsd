    </xsd:complexType>
    
    <xsd:element name="Price_Seat" type="imap-0400:Price_Seat"/>
    <xsd:element name="Price" type="imap-0400:Price_Seat"/>
    
    <xsd:complexType name="Price_Seat">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Price_Seat</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="TotalAmount" type="imap-0400:Amount">
                <xsd:annotation>
                    <xsd:documentation source="Description">Total price including taxes if any</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Source" type="ota2-dep:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">Source of pricing</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="15" minOccurs="0" ref="imap-0400:Taxes">
                <xsd:annotation>
                    <xsd:documentation source="Description">Tax applicable to this price</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PriceTypeRef" type="xsd:IDREFS">
                <xsd:annotation>
                    <xsd:appinfo>
                        <otm:OTA2EntityReference type="IDREFS">PriceType</otm:OTA2EntityReference>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Price_AndTaxDescriptionSubGrp" type="imap-0400:Price_AndTaxDescription">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Price_AndTaxDescription</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="Price_AndTaxDescription"
        substitutionGroup="imap-0400:Price_AndTaxDescriptionSubGrp" type="imap-0400:Price_AndTaxDescription"/>
    <xsd:element name="Price_AndTaxDescriptionSummary"
        substitutionGroup="imap-0400:Price_AndTaxDescriptionSubGrp" type="imap-0400:Price_AndTaxDescription"/>
    <xsd:element name="Price_AndTaxDescriptionDetail"
        substitutionGroup="imap-0400:Price_AndTaxDescription" type="imap-0400:Price_AndTaxDescription_Detail"/>
    
    <xsd:element name="TaxTypeSubGrp" type="imap-0400:Price_AndTaxDescription">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Price_AndTaxDescription</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TaxType"
        substitutionGroup="imap-0400:TaxTypeSubGrp" type="imap-0400:Price_AndTaxDescription"/>
    <xsd:element name="TaxTypeSummary"
        substitutionGroup="imap-0400:TaxTypeSubGrp" type="imap-0400:Price_AndTaxDescription"/>
    <xsd:element name="TaxTypeDetail"
        substitutionGroup="imap-0400:TaxType" type="imap-0400:Price_AndTaxDescription_Detail"/>
    
    <xsd:element name="PriceTypeSubGrp" type="imap-0400:Price_AndTaxDescription">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Price_AndTaxDescription</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PriceType"
        substitutionGroup="imap-0400:PriceTypeSubGrp" type="imap-0400:Price_AndTaxDescription"/>
    <xsd:element name="PriceTypeSummary"
        substitutionGroup="imap-0400:PriceTypeSubGrp" type="imap-0400:Price_AndTaxDescription"/>
    <xsd:element name="PriceTypeDetail"
        substitutionGroup="imap-0400:PriceType" type="imap-0400:Price_AndTaxDescription_Detail"/>
    
    <xsd:simpleType name="Price_AndTaxDescription_Simple">
        <xsd:annotation>
            <xsd:documentation source="Description">Price or tax type description</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Price_AndTaxDescription</otm:OTA2Entity>
                <otm:Example context="SeatMap">PIL</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="stl2:String_AlphaNumeric"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="Price_AndTaxDescription_Simple_List">
        <xsd:list itemType="imap-0400:Price_AndTaxDescription_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="Price_AndTaxDescription">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Price_AndTaxDescription</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="Code" type="stl2:String_AlphaNumeric">
                <xsd:annotation>
                    <xsd:documentation source="Description">Price or Tax Type primary code</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Description" type="ota2-dep:String_Long">
                <xsd:annotation>
                    <xsd:appinfo>
                        <otm:Example context="SeatMap">Pillow</otm:Example>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:ID" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">XML ID value used to reference this amenity.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="Price_AndTaxDescription_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Price_AndTaxDescription</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="imap-0400:Price_AndTaxDescription">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="SecondaryCode" type="imap-0400:Code"/>
                    <xsd:element minOccurs="0" name="SupplierCode" type="imap-0400:Code"/>
                    <xsd:element minOccurs="0" name="URI" type="xsd:anyURI">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Link to description of the Amenity</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="Price_Tax" type="imap-0400:Price_Tax"/>
    <xsd:element name="Taxes" type="imap-0400:Price_Tax"/>
    
    <xsd:complexType name="Price_Tax">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Price_Tax</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Tax" type="imap-0400:Amount"/>
            <xsd:element minOccurs="0" name="TaxTypeRef" type="xsd:IDREFS">
                <xsd:annotation>
                    <xsd:appinfo>
                        <otm:OTA2EntityReference type="IDREFS">TaxType</otm:OTA2EntityReference>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Seat_Air" type="imap-0400:Seat_Air"/>
    <xsd:element name="Seat" type="imap-0400:Seat_Air"/>
    
    <xsd:complexType name="Seat_Air">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Seat_Air</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Number" type="imap-0400:SeatLetter">
                <xsd:annotation>
                    <xsd:documentation source="Description">Upper case letter which when added to the row number creates a "Seat Number"</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">SeatNumber</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="99" minOccurs="0" ref="imap-0400:Occupation">
                <xsd:annotation>
                    <xsd:documentation source="Description">Seat Occupation type</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">SeatAvailability</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="99" minOccurs="0" ref="imap-0400:Location">
                <xsd:annotation>
                    <xsd:documentation source="Description">Details pertinent to location of seat in aircraft</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="99" minOccurs="0" ref="imap-0400:Limitations">
                <xsd:annotation>
                    <xsd:documentation source="Description">List of limitations associated with seat</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="99" minOccurs="0" ref="imap-0400:Facilities">
                <xsd:annotation>
                    <xsd:documentation source="Description">Facilities related to a specific seat</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="9" minOccurs="0" ref="imap-0400:Price">
                <xsd:annotation>
                    <xsd:documentation source="Description">Price of seat</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="9" minOccurs="0" ref="imap-0400:PassengerEntitlementAndPrice">
                <xsd:annotation>
                    <xsd:documentation source="Description">Passenger Specific Entitlement and Price.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="imap-0400:Bilateral">
                <xsd:annotation>
                    <xsd:documentation source="Description">Bilaterally agreed non standard seat characteristics</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AmenityRef" type="xsd:IDREFS">
                <xsd:annotation>
                    <xsd:documentation source="Description">List of Amenities that apply to this seat</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="blockCode" type="ota2-dep:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Special block code, for instance VA's "FF", "GG" special blocks.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="occupiedInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Seat is available unless present and true. Actual availability is subject to occupation rule.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="inoperativeInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">If true, this seat is not operational.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}boolean</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="premiumInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">If true, this seat is a premium seat that may be subjected to additional fees.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}boolean</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="chargeableInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Padis 9825 - CH</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="exitRowInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">This seat is on an exit row</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="restrictedReclineInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Padis code 1D</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="noInfantInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Padis code - 1A</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Cabin_Facilities" type="imap-0400:Cabin_Facilities"/>
    <xsd:element name="RowFacility" type="imap-0400:Cabin_Facilities"/>
    <xsd:element name="CabinFacility" type="imap-0400:Cabin_Facilities"/>
    
    <xsd:complexType name="Cabin_Facilities">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Cabin_Facilities</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Location" type="imap-0400:Enum_CabinFacilitiesRowLocationtype">
                <xsd:annotation>
                    <xsd:documentation source="Description">position on the aircraft</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="99" minOccurs="0" ref="imap-0400:Facility"/>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Row_Air" type="imap-0400:Row_Air"/>
    <xsd:element name="Row" type="imap-0400:Row_Air"/>
    
    <xsd:complexType name="Row_Air">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Row_Air</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="RowNumber" type="xsd:positiveInteger">
                <xsd:annotation>
                    <xsd:documentation source="Description">Row Number on Aircraft 1-999</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">RowNumber</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="100" minOccurs="0" name="Type" type="imap-0400:Enum_SeatRowType">
                <xsd:annotation>
                    <xsd:documentation source="Description">Row Characteristics i.e E for EXIT</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">RowCharacteristics</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="100" minOccurs="0" ref="imap-0400:Price">
                <xsd:annotation>
                    <xsd:documentation source="Description">Seat Price Applicable to this ROW overrides cabin level price.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="4999" minOccurs="0" ref="imap-0400:RowFacility">
                <xsd:annotation>
                    <xsd:documentation source="Description">Cabin facilities applicable to this ROW</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="99" minOccurs="0" ref="imap-0400:Seat">
                <xsd:annotation>
                    <xsd:documentation source="Description">Seat Details in this ROW</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AmenityRef" type="xsd:IDREFS">
                <xsd:annotation>
                    <xsd:documentation source="Description">List of Amenities that apply to this Row</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="AmenitySubGrp" type="imap-0400:Amenity">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Amenity</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="Amenity"
        substitutionGroup="imap-0400:AmenitySubGrp" type="imap-0400:Amenity"/>
    <xsd:element name="AmenitySummary"
        substitutionGroup="imap-0400:AmenitySubGrp" type="imap-0400:Amenity"/>
    <xsd:element name="AmenityDetail"
        substitutionGroup="imap-0400:Amenity" type="imap-0400:Amenity_Detail"/>
    
    <xsd:simpleType name="Amenity_Simple">
        <xsd:annotation>
            <xsd:documentation source="Description">Amenity</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Amenity</otm:OTA2Entity>
                <otm:Example context="SeatMap">PIL</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="stl2:String_AlphaNumeric"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="Amenity_Simple_List">
        <xsd:list itemType="imap-0400:Amenity_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="Amenity">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Amenity</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="Code" type="stl2:String_AlphaNumeric">
                <xsd:annotation>
                    <xsd:documentation source="Description">Product code.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Description" type="ota2-dep:String_Long">
                <xsd:annotation>
                    <xsd:documentation source="Description">Description of the amenity</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Fee" type="imap-0400:Amount">
                <xsd:annotation>
                    <xsd:documentation source="Description">Total Fee if any applicable to the amenity</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:ID" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">XML ID value used to reference this amenity.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="Amenity_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Amenity</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="imap-0400:Amenity">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="SecondaryCode" type="imap-0400:Code">
                        <xsd:annotation>
                            <xsd:documentation source="Description">sub-code or additional code</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="SupplierCode" type="imap-0400:Code">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Supplier specific product code.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="15" minOccurs="0" ref="imap-0400:Taxes">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Tax applicable to this price</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="URI" type="xsd:anyURI">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Link to description of the Amenity</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="PriceTypeRef" type="xsd:IDREF">
                        <xsd:annotation>
                            <xsd:documentation source="Description">List of price types applicable</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="AmenityTable" type="imap-0400:AmenityTable"/>
    
    <xsd:complexType name="AmenityTable">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">AmenityTable</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="1000" minOccurs="0" ref="imap-0400:AmenitySubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Table of Amenities to be referenced from the seat map.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Price_Table" type="imap-0400:Price_Table"/>
    <xsd:element name="PriceList" type="imap-0400:Price_Table"/>
    
    <xsd:complexType name="Price_Table">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Price_Table</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="1000" minOccurs="0" ref="imap-0400:PriceTypeSubGrp"/>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Price_TaxTable" type="imap-0400:Price_TaxTable"/>
    <xsd:element name="TaxTable" type="imap-0400:Price_TaxTable"/>
    
    <xsd:complexType name="Price_TaxTable">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Price_TaxTable</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="1000" minOccurs="0" ref="imap-0400:TaxTypeSubGrp"/>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Cabin" type="imap-0400:Cabin"/>
    
    <xsd:simpleType name="Cabin_Simple">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Cabin</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="imap-0400:Enum_CabinClassLocation"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="Cabin_Simple_List">
        <xsd:list itemType="imap-0400:Cabin_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="Cabin">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Cabin</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" ref="imap-0400:CabinClass">
                <xsd:annotation>
                    <xsd:documentation source="Description">Cabin class/type/marketing name information</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="100" minOccurs="0" ref="imap-0400:Price">
                <xsd:annotation>
                    <xsd:documentation source="Description">Default cabin level seat price with qualifiers and rules.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="999" minOccurs="0" ref="imap-0400:Row">
                <xsd:annotation>
                    <xsd:documentation source="Description">Row and Seat Data</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="imap-0400:Wing">
                <xsd:annotation>
                    <xsd:documentation source="Description">overwing row definition</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="99" minOccurs="0" ref="imap-0400:Column">
                <xsd:annotation>
                    <xsd:documentation source="Description">Column descriptors</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="4999" minOccurs="0" ref="imap-0400:CabinFacility">
                <xsd:annotation>
                    <xsd:documentation source="Description">cabin facilities relative to cabin and first and last row</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="99" minOccurs="0" ref="imap-0400:Section">
                <xsd:annotation>
                    <xsd:documentation source="Description">Separation of sections in a single cabin</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AmenityRef" type="xsd:IDREFS">
                <xsd:annotation>
                    <xsd:appinfo>
                        <otm:OTA2EntityReference type="IDREFS">Amenity</otm:OTA2EntityReference>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="capacity" type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The total quantity of  physical seats in the cabin.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">246</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="firstRow" type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">number of first row in this cabin</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">07</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="lastRow" type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">number of last row in this cabin</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">36</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="classLocation"
            type="imap-0400:Enum_CabinClassLocation" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Class location in cabin Lowerdeck,Maindeck or Upperdeck</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="seatOccupationDefault"
            type="imap-0400:Enum_SeatOccupationCabinDefault" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">default occupation for all seats</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="rowQty" type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Total number of Rows this cabin</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">25</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="POS" type="imap-0400:POS"/>
    
    <xsd:complexType name="POS">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">POS</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Actual" type="imap-0400:POS_Location">
                <xsd:annotation>
                    <xsd:documentation source="Description">Actual is where the transaction is initiated.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Home" type="imap-0400:POS_Location">
                <xsd:annotation>
                    <xsd:documentation source="Description">Home is assigned to the agent account.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PCC" type="stl2:Code_PseudoCity">
                <xsd:annotation>
                    <xsd:documentation source="Description">Psuedo is either Agency PCC or Agent AAA. Required for
						most source code is provided.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ClientContext" type="imap-0400:ClientContext">
                <xsd:annotation>
                    <xsd:documentation source="Description">Client Context</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="duty" type="stl2:Enum_CodeDuty" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">An authority code assigned to a requestor.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="LNIATA" type="stl2:Lniata" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">LNIATA of the Console the agent signed into.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">ABCD02</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sine" type="stl2:AgentSine" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Identifies the agent or party within the requesting
						entity.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="multiHost" type="stl2:Code_Airline" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Airline partition for multi-host source locations.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="company" type="stl2:Code_Agency" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The IATA assigned code for the Agency or Airine or GDS</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Seat_Bilateral" type="imap-0400:Seat_Bilateral"/>
    <xsd:element name="Bilateral" type="imap-0400:Seat_Bilateral"/>
    
    <xsd:complexType name="Seat_Bilateral">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Seat_Bilateral</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="99" minOccurs="0"
                name="Characteristic" type="stl2:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">Non Padis or OTA standard seat characteristics bilaterally agreed to support with a partner airline. Airline partner specific</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Cabin_Section" type="imap-0400:Cabin_Section"/>
    <xsd:element name="Section" type="imap-0400:Cabin_Section"/>
    
    <xsd:complexType name="Cabin_Section">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Cabin_Section</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:attribute name="section" type="stl2:String_Tiny" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">Section defined within a Cabin</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">B</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="firstRow" type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">first row of this section</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">16</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="lastRow" type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">last row of this section</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">19</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Cabin_Type" type="imap-0400:Cabin_Type"/>
    <xsd:element name="CabinClass" type="imap-0400:Cabin_Type"/>
    <xsd:element name="CabinDefinition" type="imap-0400:Cabin_Type"/>
    
    <xsd:complexType name="Cabin_Type">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Cabin_Type</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="CabinType" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Cabin Type for RQ/RS eq. First , Business, Economy</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="RBD" type="stl2:RBD">
                <xsd:annotation>
                    <xsd:documentation source="Description">Reservation Booking Designator to apply to seatmap cabin request</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="MarketingName" type="ota2-dep:String_Short">
                <xsd:annotation>
                    <xsd:documentation source="Description">Any Marketing Name to be displayed instead of just First or Economy</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="99" minOccurs="0"
                name="MarketingDescription" type="ota2-dep:String_Short">
                <xsd:annotation>
                    <xsd:documentation source="Description">Any free text description or banner applicable to the cabin</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="JourneyFlight" type="imap-0400:JourneyFlight"/>
    
    <xsd:complexType name="JourneyFlight">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">JourneyFlight</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" ref="imap-0400:Flight"/>
            <xsd:element minOccurs="0" ref="imap-0400:FlightJourneyData"/>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="JourneyData" type="imap-0400:JourneyData"/>
    
    <xsd:complexType name="JourneyData">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">JourneyData</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="999" minOccurs="0" ref="imap-0400:JourneyFlight"/>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="TravelerFareInfo" type="imap-0400:TravelerFareInfo"/>
    
    <xsd:simpleType name="TravelerFareInfo_Simple">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TravelerFareInfo</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="ota2:Empty"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="TravelerFareInfo_Simple_List">
        <xsd:list itemType="imap-0400:TravelerFareInfo_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="TravelerFareInfo">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TravelerFareInfo</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="TypeTotalPrice" type="imap-0400:Amount">
                <xsd:annotation>
                    <xsd:documentation source="Description">Total price for all passengers of the same type</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">TypeTotalPrice</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="Base" type="imap-0400:Amount">
                <xsd:annotation>
                    <xsd:documentation source="Description">Total base fare amount that includes surcharges (if applicable).
                                           Example: 200000</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">BaseFare</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="Taxes" type="imap-0400:Amount">
                <xsd:annotation>
                    <xsd:documentation source="Description">Total taxes amount.  Example: 50000</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">Taxes</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="Total" type="imap-0400:Amount">
                <xsd:annotation>
                    <xsd:documentation source="Description">Total price: 50000</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">Total Price</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TicketByDate" type="stl2:String_Short">
                <xsd:annotation>
                    <xsd:documentation source="Description">Ticket by date/ time.Example: 2013-12-25.this is not the Booking Time Limit (BookByDate) that applies to offers in a shopping response. This is the date/ time when a ticket-associated offer must be purchased/ ticketed</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">TicketByDate</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="TypeRequested" type="ota2-dep:String_Short" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">Passenger type being requested for pricing.  If the passenger type sent by the GDS is not in the below predefined list, then it is shown as is. Predefined values are:
                                       ADT= Adult
                                       CHD = Child
                                       INF = Infant
                                       GOV = Government
                                       MIL = Military
                                       SRC = Senior citizen
                                       STD = Student
                                       YTH = Youth
                                       Example: CHD</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="SeatMap">TypeRequested</otm:Equivalent>
                    <otm:Example context="SeatMap">ADT</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TypePriced" type="ota2-dep:String_Short" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">Passenger type being priced. This type can be different from the type requested if there is no available fare for the requested type.  For example the requested type is GOV and the priced type is ADT because there is no available fare for type GOV and there is an available fare for type ADT.  If the passenger type sent by the GDS is not the below predefined list, then it is shown as is.  Predefined values are:
                                        ADT= Adult
                                        CHD = Child
                                        INF = Infant
                                        GOV = Government
                                        MIL = Military
                                        SRC = Senior citizen
                                        STD = Student
                                        YTH = Youth
                                        Example: ADT</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="SeatMap">TypePriced</otm:Equivalent>
                    <otm:Example context="SeatMap">ADT</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TypeCount" type="xsd:integer" use="required">
            <xsd:annotation>
                <xsd:appinfo>
                    <otm:Equivalent context="SeatMap">TypeCount</otm:Equivalent>
                    <otm:Example context="SeatMap">1</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="FareGroup" type="imap-0400:FareGroup"/>
    
    <xsd:complexType name="FareGroup">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">FareGroup</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="GroupTotalPrice" type="imap-0400:Amount">
                <xsd:annotation>
                    <xsd:documentation source="Description">Total price for the entire itinerary for all passengers of all types.  It is the sum of all TypeTotalPrice.  Example: 320000</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">TotalPrice</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="999" minOccurs="1" ref="imap-0400:TravelerFareInfo"/>
            <xsd:element minOccurs="1" name="ValidatingCarrier" type="stl2:Code_Airline">
                <xsd:annotation>
                    <xsd:documentation source="Description">Validating carrier airline designator code..</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="SeatMap">ValidatingCarrier</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="SeatMapSubGrp" type="imap-0400:SeatMap_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">SeatMap</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SeatMapID"
        substitutionGroup="imap-0400:SeatMapSubGrp" type="imap-0400:SeatMap_ID"/>
    <xsd:element name="SeatMapIdentifier"
        substitutionGroup="imap-0400:SeatMapSubGrp" type="imap-0400:SeatMap_ID"/>
    <xsd:element name="SeatMap" substitutionGroup="imap-0400:SeatMapID" type="imap-0400:SeatMap"/>
    <xsd:element name="SeatMapSummary"
        substitutionGroup="imap-0400:SeatMapSubGrp" type="imap-0400:SeatMap"/>
    <xsd:element name="SeatMapQueryEnhanced" type="imap-0400:SeatMap_Query_Enhanced"/>
    
    <xsd:complexType name="SeatMap_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">SeatMap</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Equipment" type="ota2-dep:Code_IATA_AircraftType">
                <xsd:annotation>
                    <xsd:documentation source="Description">IATA aircraft type</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="mapID" type="stl2:Identifier_oid" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Unique ID that can be used to reference this seatmap</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">sm33493-dke3299</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="system" type="ota2-dep:String_Long" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">System that provided the mapID value.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">Merch1</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="SeatMap">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">SeatMap</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="imap-0400:SeatMap_ID">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="RequestType" type="imap-0400:Request">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Stateful, Stateless, Payload</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Currency" type="ota2-dep:Code_ISO_Currency">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Default Currency for the map</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="imap-0400:Flight">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Flight date and time</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="99" minOccurs="0" ref="imap-0400:FareAvailQualifiers">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Seat map prices and availabity are based on traveller charactistics.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="9" minOccurs="1" ref="imap-0400:Cabin">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Seatmap details for cabin and Row and Seat</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="4999" minOccurs="0" ref="imap-0400:AmenityTable">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Amenities referenced by ID applicable to complete request</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="imap-0400:PriceList"/>
                    <xsd:element maxOccurs="1000" minOccurs="0" ref="imap-0400:TaxTable">
                        <xsd:annotation>
                            <xsd:documentation source="Description">List of descriptions of taxes applied to seat prices.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="imap-0400:POS">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Booking agency POS information</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
                </xsd:sequence>
                <xsd:attribute name="correlationID"
                    type="stl2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">An ID for end-to-end correlation of log messages with the corresponding Web service message throughout the processing of the Web service message.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="SeatMap">12345678</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="changeOfGaugeInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">if indicator is present the flight undergoes a change of guage on its path</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="SeatMap_Query_Enhanced">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">SeatMap</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="RequestType" type="imap-0400:Request">
                <xsd:annotation>
                    <xsd:documentation source="Description">Type of request Staeful, Stateless, Payload</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" ref="imap-0400:Flight">
                <xsd:annotation>
                    <xsd:documentation source="Description">Flight number , date and time.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="imap-0400:CabinDefinition">
                <xsd:annotation>
                    <xsd:documentation source="Description">Class Cabin being requeste</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Currency" type="ota2-dep:Code_ISO_Currency">
                <xsd:annotation>
                    <xsd:documentation source="Description">currency request for seatmap pricing</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AirlineRecordLocator" type="imap-0400:PNRLOC">
                <xsd:annotation>
                    <xsd:documentation source="Description">Airline Record Locator of passenger or group</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="99" minOccurs="0" ref="imap-0400:FareAvailQualifiers">
                <xsd:annotation>
                    <xsd:documentation source="Description">Seat map prices and availabity are based on traveller charactistics.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="imap-0400:POS">
                <xsd:annotation>
                    <xsd:documentation source="Description">Booking agency POS information</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="imap-0400:JourneyData">
                <xsd:annotation>
                    <xsd:documentation source="Description">Journey control information.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="imap-0400:FareGroup">
                <xsd:annotation>
                    <xsd:documentation source="Description">Fare group containing all information regarding the air fares.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SeatMapSource" type="imap-0400:SeatMapSource"/>
            <xsd:element minOccurs="0" name="Client" type="imap-0400:Client"/>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Query"/>
        </xsd:sequence>
        <xsd:attribute name="correlationID" type="stl2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">An ID for end-to-end correlation of log messages with the corresponding Web service message throughout the processing of the Web service message.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="SeatMap">123456ABC</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:simpleType name="Client">
        <xsd:annotation>
            <xsd:documentation source="Description">client issuing the request</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="EnumerationClosed">Client</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="KIOSK"/>
            <xsd:enumeration value="MOBILE"/>
            <xsd:enumeration value="WEB"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="SeatMapSource">
        <xsd:annotation>
            <xsd:documentation source="Description">Seat Map type to retrieve: PRS (default) or ACS</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="EnumerationClosed">SeatMapSource</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="PRS"/>
            <xsd:enumeration value="ACS"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="ChoiceType">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="EnumerationClosed">ChoiceType</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Y"/>
            <xsd:enumeration value="N"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:element name="PassengerEntitlementAndPrice" type="imap-0400:PassengerEntitlementAndPrice"/>
    
    <xsd:complexType name="PassengerEntitlementAndPrice">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">PassengerEntitlementAndPrice</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="TravellerID" type="ota2-dep:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">Traveler Profile Identifier.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="imap-0400:Price">
                <xsd:annotation>
                    <xsd:documentation source="Description">Price of seat</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="FeeWaiverRuleId" type="ota2-dep:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">Fee Waiver Rule ID.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="EntitlementRuleId" type="ota2-dep:String_Tiny">
                <xsd:annotation>
                    <xsd:documentation source="Description">Entitlement Rule ID.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="entitledInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">This seat is entitled</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="PriceQuoteCriteria" type="imap-0400:PriceQuoteCriteria"/>
    
    <xsd:complexType name="PriceQuoteCriteria">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">PriceQuoteCriteria</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="4" minOccurs="1" ref="imap-0400:PriceQuoteDesignator">
                <xsd:annotation>
                    <xsd:documentation source="Description">Price Quote Reference Data</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="PriceQuoteDesignator" type="imap-0400:PriceQuoteDesignator"/>
    
    <xsd:complexType name="PriceQuoteDesignator">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">PriceQuoteDesignator</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:attribute name="number" type="xsd:int" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">Price Quote Number</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="EnhancedSeatMapRQ"
        substitutionGroup="stlp:STL_Request_Payload" type="imap-0400:EnhancedSeatMap_RQ"/>
    
    <xsd:complexType name="EnhancedSeatMap_RQ">
        <xsd:annotation>
            <xsd:documentation source="Description">Used to request a seatmap from another system</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Operation">EnhancedSeatMap_EnhancedSeatMap</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="stlp:STL_Request_Payload">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="imap-0400:SeatMapQueryEnhanced"/>
                    <xsd:element minOccurs="0" ref="imap-0400:PriceQuoteCriteria"/>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="EnhancedSeatMapRS"
        substitutionGroup="stlp:STL_Response_Payload" type="imap-0400:EnhancedSeatMap_RS"/>
    
    <xsd:complexType name="EnhancedSeatMap_RS">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Operation">EnhancedSeatMap_EnhancedSeatMap</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="stlp:STL_Response_Payload">
                <xsd:sequence>
                    <xsd:element maxOccurs="9" minOccurs="0" ref="imap-0400:SeatMapSubGrp"/>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="AddMapRQ"
        substitutionGroup="stlp:STL_Request_Payload" type="imap-0400:AddMap_RQ"/>
    
    <xsd:complexType name="AddMap_RQ">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Operation">EnhancedSeatMap_AddMap</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="stlp:STL_Request_Payload">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="imap-0400:SeatMapSubGrp"/>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="AddMapRS"
        substitutionGroup="stlp:STL_Response_Payload" type="imap-0400:AddMap_RS"/>
    
    <xsd:complexType name="AddMap_RS">
        <xsd:annotation>
