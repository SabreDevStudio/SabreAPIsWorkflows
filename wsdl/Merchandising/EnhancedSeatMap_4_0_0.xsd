            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Seat_Limitations</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Detail" type="imap-0400:Enum_SeatLimitationsType">
