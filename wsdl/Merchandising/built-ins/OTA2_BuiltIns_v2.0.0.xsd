    <xsd:simpleType name="Enum_TransactionStatus">
        <xsd:annotation>
            <xsd:documentation source="Description">An identifier for where this message falls within a sequence of messages.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="EnumerationClosed">Enum_TransactionStatus</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Start"/>
            <xsd:enumeration value="End"/>
            <xsd:enumeration value="Rollback"/>
            <xsd:enumeration value="InSeries"/>
            <xsd:enumeration value="Continuation"/>
            <xsd:enumeration value="Subsequent"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="CodeList">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a codelist.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">CodeList</otm:OTA2Entity>
                <otm:Example context="OTM2">OpenTravel</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">The source of the codelist.</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:Enum_CodelistSource_Base">
                <xsd:attribute name="extension" type="ota2:String_EnumExtension"/>
                <xsd:attribute name="itemCode" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The numeric ID or code for the value from the code list.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">14</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="itemName" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The literal (textual) value from the code list.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">Corporate Booking Information</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="listNameCode"
                               type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The acronym or code for the code list name.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">ADT</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="openTravelListName"
                               type="ota2:Enum_OpenTravelCodelist" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The OpenTravel code list name.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">ADT:Additional Detail Type</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="otherListName"
                               type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The name of a third party (non-OpenTravel) code list.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">IATA Country Codes</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="ContextualCode">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a contextual code.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">ContextualCode</otm:OTA2Entity>
                <otm:Example context="OTM2">ABC123</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">A code up to 32 characters in length.</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:String_Tiny">
                <xsd:attribute name="context" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The named context of the code.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">Owner</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="URL" type="ota2:URL" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A URL to the source owner of the code.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">http://www.ABCCompany.com/somesource</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="ContextualID">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a contextual identifier.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">ContextualID</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">A unique ID up to 128 characters in length.</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:String_Short">
                <xsd:attribute name="context" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The named context of the ID.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">Owner</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="URL" type="ota2:URL" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A URL to the source owner of the ID.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">http://www.opentravel.org</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="ExchangeRate">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes an exchange rate.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">ExchangeRate</otm:OTA2Entity>
                <otm:Example context="OTM2">USD</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">An ISO 4217 currency code for the base currency that's being converted.
                </xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:Code_ISO_Currency">
                <xsd:attribute name="baseNUC" type="ota2:Money" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The base neutral unit of construction amount.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">100</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="exchangeDateTime"
                               type="ota2:Date_DateTime" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The date and time of the conversion rate.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">2012-12-17T09:30:47</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="exchangeRate" type="xsd:decimal" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The rate used for conversion from the source currency to the target
                            currency.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">1.32932</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="exchangeSourceName"
                               type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The name of the exchange rate source.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">Exchange Rates.com</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="exchangeSourceURL" type="ota2:URL" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A URL for the source of the conversion rate.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">http://www.exchangerates.org.uk/US-Dollar-USD-currency-table.html</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="toCurrency"
                               type="ota2:Code_ISO_Currency" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">An ISO 4217 currency code for the final converted currency.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">EUR</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="InitiatorAirline">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes airline information for a message requestor or sender.
            </xsd:documentation>
            <xsd:documentation source="Reference">http://bit.ly/OTA2_IATA_AirlineCode</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">InitiatorAirline</otm:OTA2Entity>
                <otm:Example context="OTM2">UA</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">A two character IATA airline vendor ID.</xsd:documentation>
                <xsd:documentation source="Reference">http://bit.ly/OTA2_IATA_AirlineCode</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:Code_IATA_AirlineID">
                <xsd:attribute name="airportCode"
                               type="ota2:Code_IATA_Airport" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A three character IATA assigned airport code.</xsd:documentation>
                        <xsd:documentation source="Reference">http://bit.ly/OTA2_IATA_AirportCodes</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">JFK</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="erspCode"
                               type="ota2:Code_IATA_ERSP" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The IATA airline Electronic Reservation Services Provider (ERSP) code.
                        </xsd:documentation>
                        <xsd:documentation source="Reference">http://bit.ly/OTA2_ERSP</xsd:documentation>
                        <xsd:documentation source="Implementer">An ERSP is a unique IATA (in cooperation with ATA and ARC) code used as a
                            means of identification when performing online bookings with airlines.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">123456789</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="firstDeparturePoint"
                               type="ota2:Code_IATA_City" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">An IATA city code for the first departure point for a trip.
                        </xsd:documentation>
                        <xsd:documentation source="Reference">http://bit.ly/OTA2_IATA_AirportCodes</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">JFK</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="InitiatorBookingChannel">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a booking channel.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">InitiatorBookingChannel</otm:OTA2Entity>
                <otm:Example context="OTM2">Agent</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">An extensible list of booking channel types.</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:Enum_BookingChannel_Base">
                <xsd:attribute name="extension" type="ota2:String_EnumExtension"/>
                <xsd:attribute name="code" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A code for the booking channel.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">12345</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="id" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A unique ID for the booking channel that is typically used for message
                            authentication.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">ABC1234567</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="name" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The name of the booking channel.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">The Best Travel Company Public Website</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="primaryInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, the booking channel type is the primary means of connectivity used
                            by the source.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="InitiatorID">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a unique ID and contextual information for the ID for a message
                requestor or sender.
            </xsd:documentation>
            <xsd:documentation source="Implementer">The ID and associated information should be known between trading partners.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">InitiatorID</otm:OTA2Entity>
                <otm:Example context="OTM2">vendor12345</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">A unique identifier for the requestor or sender of the message.</xsd:documentation>
                <xsd:documentation source="Implementer">This ID should be known between trading partners.</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:String_Short">
                <xsd:attribute name="context" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The contextual source of the requestor ID, such as IATA, ARC, and ERSP.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">IATA</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="url" type="ota2:URL" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A URL that identifies the location associated with the @context.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">http://www.vendor12345.com</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="InitiatorTravelAgent">
        <xsd:annotation>
            <xsd:documentation source="Description">Sine ID and other metadata about a travel agency.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">InitiatorTravelAgent</otm:OTA2Entity>
                <otm:Example context="OTM2">7854JL</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">The travel agent sine identifier.</xsd:documentation>
                <xsd:documentation source="Implementer">A code used to identify a user or a travel agency, typically used by GDS.
                </xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:String_Tiny">
                <xsd:attribute name="companyName"
                               type="ota2:Name_Proper" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The name of the travel agency.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">ABC Travel Company</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="dutyCode"
                               type="ota2:Code_AgentDuty" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A two to five character code (alpha, numeric or combination) code that
                            specifies a travel agents duty code used for agency authentication.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">GS</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="pseudoCityCode"
                               type="ota2:Code_PseudoCity" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A three to five character code (alpha, numeric or combination) that
                            specifies a GDS code used to identify a travel agency location.
                        </xsd:documentation>
                        <xsd:documentation source="Implementer">Note that this may be a required field when using negotiated rates to
                            associate the rates to an agency.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">ORD</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="terminalID"
                               type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The electronic address of the device from which information is entered.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">01989</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="Identifier_Country">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a country.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Identifier_Country</otm:OTA2Entity>
                <otm:Example context="OTM2">USA</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">A three character alpha country code defined in ISO 3166 format.</xsd:documentation>
                <xsd:documentation source="Reference">https://en.wikipedia.org/wiki/ISO_3166-1_alpha-3</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:Code_ISO_Country">
                <xsd:attribute name="alpha2" type="ota2:String_Alpha2" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The two character alpha country code defined in ISO 3166 format.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">FR</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="fullName" type="ota2:Name_Proper" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The name of the country.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">France</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="Identifier_Currency">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a currency.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Identifier_Currency</otm:OTA2Entity>
                <otm:Example context="OTM2">USD</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">A three character ISO 4217 currency code with other related metadata.
                </xsd:documentation>
                <xsd:documentation source="Reference">http://bit.ly/OTA2_ISO4217</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:Code_ISO_Currency">
                <xsd:attribute name="decimalPlaces"
                               type="xsd:positiveInteger" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">An ISO 4217 standard "minor unit" for the number of decimal places for the
                            associated currency.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">2</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="pricingFilingInd"
                               type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, this is the pricing/filing currency.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="Identifier_Language">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a language.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Identifier_Language</otm:OTA2Entity>
                <otm:Example context="OTM2">en</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">A code for a language in ISO 639-1 (2 alpha character) format.</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:Code_ISO_Language">
                <xsd:attribute name="alpha3" type="ota2:String_Alpha3" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The three character alpha language code.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">ENG</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="name" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The name of the language for display purposes.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">ENGLISH</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="primaryInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, this is the primary (or preferred) language within a set of
                            specified languages.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="MessagePassword">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a message password.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">MessagePassword</otm:OTA2Entity>
                <otm:Example context="OTM2">eK7@41!3aB</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">A password associated with a message request.</xsd:documentation>
                <xsd:documentation source="Implementer">Note that it is an OpenTravel Best Practice to encrpt any user identifying,
                    personally identify or account access data.
                </xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:String_Short">
                <xsd:attribute name="domain" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The domain associated with the message password.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">CRS</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="Processing_Currency">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes the currency used in a request message and the preferred
                response currency.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Processing_Currency</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">Request and response currency information.</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:Code_ISO_Currency">
                <xsd:attribute name="decimalPlaces"
                               type="xsd:positiveInteger" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">An ISO 4217 standard "minor unit" for the number of decimal places for the
                            associated currency.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">2</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="initiatorInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, this is the currency that the initiating message is in.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="responseInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, this is the requested currency for message responses.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="pricingFilingInd"
                               type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, this is the pricing/filing currency.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="Processing_Language">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes the language used in a request message and the preferred
                response language.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Processing_Language</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">Request and response language information.</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:Code_ISO_Language">
                <xsd:attribute name="alpha3" type="ota2:String_Alpha3" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The three character alpha language code.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">ENG</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="name" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The name of the language for display purposes.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">ENGLISH</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="initiatorInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, this is the language that the initiating message is in.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="responseInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, this is the requested language for message responses.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="primaryInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, this is the primary (or preferred) language within a set of
                            specified languages.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="TargetSystem">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a target (or receiving) processing system.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">TargetSystem</otm:OTA2Entity>
                <otm:Example context="OTM2">Production</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">An extensible list of target message processing system types..</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:Enum_TargetProcessingSystem_Base">
                <xsd:attribute name="extension" type="ota2:String_EnumExtension"/>
                <xsd:attribute name="systemName"
                               type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The name of the target system that may be specified if there are more than
                            one system for the specified system type.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">Server3</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="TimeWindow">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a date and optional time with a time period that can be applied
                before and/or after the date.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">TimeWindow</otm:OTA2Entity>
                <otm:Example context="OTM2">2012-12-17T09:30:47</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">The date and optional time.</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:Date_DateTime">
                <xsd:attribute name="windowAfter"
                               type="ota2:Time_Duration" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A period of time that can be applied to the base date/time resulting in a
                            later range of time.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">P3D</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="windowBefore"
                               type="ota2:Time_Duration" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A period of time that can be applied to the base date/time resulting in an
                            earlier range of time.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">PT12H</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="crossDateInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, the requested period may extend over the previous or following day.
                            If false, the search period is restricted to the date/time specified.
                        </xsd:documentation>
                        <xsd:documentation source="Implementer">Note that this indicator is typically used when the window duration is in
                            hours.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="TimeZone">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a time zone.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">TimeZone</otm:OTA2Entity>
                <otm:Example context="OTM2">ET</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">The time zone name or abbreviation.</xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:String_Tiny">
                <xsd:attribute name="utcOffset" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The Coordinated Universal Time (UTC) time zone offset.</xsd:documentation>
                        <xsd:documentation source="Implementer">A UTC offset is typically represented with a plus or minus sign, an hour
                            increment and an hour designator, such as - 5h.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">- 5h</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="daylightSavingsInd"
                               type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, daylight (or summer) savings time is used in the time zone.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="Transaction">
        <xsd:annotation>
            <xsd:documentation source="Description">Metadata that describes a message transaction.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Transaction</otm:OTA2Entity>
                <otm:Example context="OTM2">12345</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:annotation>
                <xsd:documentation source="Description">The transaction ID.</xsd:documentation>
                <xsd:documentation source="Implementer">Unique identifier to relate all messages within a transaction (e.g. this would be
                    sent in all request and response messages that are part of an on-going transaction).
                </xsd:documentation>
                <xsd:documentation source="Implementer">This would be sent in all request and response messages that are part of an on-going
                    transaction.
                </xsd:documentation>
            </xsd:annotation>
            <xsd:extension base="ota2:String_Short">
                <xsd:attribute name="correlationID"
                               type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">An ID for end-to-end correlation of log messages with the corresponding Web
                            service message throughout the processing of the Web service message.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">6712</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="sequenceNumber"
                               type="xsd:positiveInteger" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The sequence number of the transaction assigned by the sending system for
                            ordered message processing or message resynchronization.
                        </xsd:documentation>
                        <xsd:documentation source="Implementer">Message resynchronization typically is required if a system has been
                            off-line and needs to retrieve messages that were missed.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">6</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="status"
                               type="ota2:Enum_TransactionStatus" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Where this message falls within a sequence of messages.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">Start</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="retransmissionInd"
                               type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, the message is being re-sent after a failure.</xsd:documentation>
                        <xsd:documentation source="Implementer">It is recommended that this attribute is used (i.e., set to TRUE) only when
                            a message is retransmitted.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:element name="DataPolicySubGrp" type="ota2:DataPolicy">
        <xsd:annotation>
            <xsd:documentation source="Description">Information about data policy(s) that may apply to all or a portion of the message
                contents.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">DataPolicy</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="DataPolicy"
                 substitutionGroup="ota2:DataPolicySubGrp" type="ota2:DataPolicy"/>
    <xsd:element name="DataPolicySummary"
                 substitutionGroup="ota2:DataPolicySubGrp" type="ota2:DataPolicy"/>
    <xsd:element name="DataPolicyDetail"
                 substitutionGroup="ota2:DataPolicy" type="ota2:DataPolicy_Detail"/>

    <xsd:complexType name="DataPolicy">
        <xsd:annotation>
            <xsd:documentation source="Description">Summary data policy information.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">DataPolicy</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="type" type="ota2:Enum_DataPolicyType">
                <xsd:annotation>
                    <xsd:documentation source="Description">An extensible list of data policy types.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="schemaFieldXPath" type="ota2:String_Medium" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">An xPath to the tag of the schema element or attribute (field) that the data
                    policy(s) applies to.
                </xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="OTM2">AirInventoryQueryRQ/Traveler/@birthDate</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="DataPolicy_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Detail data policy information.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">DataPolicy</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ota2:DataPolicy">
                <xsd:attribute name="description"
                               type="ota2:String_Medium" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A brief description of the data policy intended to be used for logging and
                            compliance purposes.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">Restricted to internal sharing as needed-no third party sharing.</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="url" type="ota2:URL" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A URL to a source that describes the data policy.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">http://www.BTCCO.com/datapolicy.aspx</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:element name="EncryptionSubGrp" type="ota2:Encryption">
        <xsd:annotation>
            <xsd:documentation source="Description">Information about encrypted elements and attributes within the message.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Encryption</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="Encryption"
                 substitutionGroup="ota2:EncryptionSubGrp" type="ota2:Encryption"/>
    <xsd:element name="EncryptionSummary"
                 substitutionGroup="ota2:EncryptionSubGrp" type="ota2:Encryption"/>
    <xsd:element name="EncryptionDetail"
                 substitutionGroup="ota2:Encryption" type="ota2:Encryption_Detail"/>

    <xsd:complexType name="Encryption">
        <xsd:annotation>
            <xsd:documentation source="Description">Summary payload information.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Encryption</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:attribute name="schemaFieldXPath" type="ota2:String_Medium" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">An xPath to the tag of the schema element or attribute (field) that is encrypted.
                </xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="OTM2">AirInventoryQueryRQ/Traveler/@birthDate</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="value" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">The encrypted value.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="Encryption_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Detail encryption information.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Encryption</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ota2:Encryption">
                <xsd:attribute name="method" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The encryption method being used.</xsd:documentation>
                        <xsd:documentation source="Implementer">Note that if you do not specify the encryption method, it should be known to
                            your trading partners.
                        </xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="OTM2">Triple DES</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:element name="TransactionInitiatorSubGrp" type="ota2:TransactionInitiator">
        <xsd:annotation>
            <xsd:documentation source="Description">Transaction initiator identification, authentication and other associated summary and
                detail information.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TransactionInitiator</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TransactionInitiator"
                 substitutionGroup="ota2:TransactionInitiatorSubGrp" type="ota2:TransactionInitiator"/>
    <xsd:element name="TransactionInitiatorSummary"
                 substitutionGroup="ota2:TransactionInitiatorSubGrp" type="ota2:TransactionInitiator"/>
    <xsd:element name="TransactionInitiatorDetail"
                 substitutionGroup="ota2:TransactionInitiator" type="ota2:TransactionInitiator_Detail"/>

    <xsd:element name="PointOfSaleSubGrp" type="ota2:TransactionInitiator">
        <xsd:annotation>
            <xsd:documentation source="Description">Transaction initiator identification, authentication and other associated summary and
                detail information.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TransactionInitiator</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PointOfSale"
                 substitutionGroup="ota2:PointOfSaleSubGrp" type="ota2:TransactionInitiator"/>
    <xsd:element name="PointOfSaleSummary"
                 substitutionGroup="ota2:PointOfSaleSubGrp" type="ota2:TransactionInitiator"/>
    <xsd:element name="PointOfSaleDetail"
                 substitutionGroup="ota2:PointOfSale" type="ota2:TransactionInitiator_Detail"/>

    <xsd:complexType name="TransactionInitiator">
        <xsd:annotation>
            <xsd:documentation source="Description">Summary transaction initiator information.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TransactionInitiator</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="ID" type="ota2:InitiatorID">
                <xsd:annotation>
                    <xsd:documentation source="Description">A unique identifier for the requestor or sender of the message with other
                        metadata related to the ID.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="Type" type="ota2:Enum_UniqueID_Type">
                <xsd:annotation>
                    <xsd:documentation source="Description">An extensible list of requestor or sender types.</xsd:documentation>
                    <xsd:documentation source="Implementer">This type should be known between trading partners.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="TransactionInitiator_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Detail transaction initiator information.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TransactionInitiator</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ota2:TransactionInitiator">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="Agent" type="ota2:InitiatorTravelAgent">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The travel agent sine number and other identification and authentication
                                information.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Airline" type="ota2:InitiatorAirline">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The two character airline IATA vendor ID and other identification and
                                authentication information.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="BookingChannel" type="ota2:InitiatorBookingChannel">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The type of the booking channel and other identification and
                                authentication information for the requestor or sender.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Company" type="ota2:String_Short">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Company information for the requestor or sender.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="RelativeLocation" type="ota2:String_Short">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The geographical location of the requestor.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="TimeZone" type="ota2:TimeZone">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A time zone abbreviation with other related metadata for the requestor
                                or sender.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="TravelSector" type="ota2:Enum_TravelSector">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The travel sector associated with the requestor or sender.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

</xsd:schema>
