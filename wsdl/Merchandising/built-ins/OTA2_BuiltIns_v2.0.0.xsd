<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
            elementFormDefault="qualified"
            targetNamespace="http://opentravel.org/common/v02"
            xmlns:ota2="http://opentravel.org/common/v02"
            xmlns:otm="http://www.OpenTravel.org/ns/OTA2/AppInfo_v01_00" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <xsd:annotation>
        <xsd:appinfo>
            <otm:Library>
                <otm:ProjectName>AirlineBranding.otp</otm:ProjectName>
                <otm:LibraryName>OTA2_BuiltIns_v2.0.0</otm:LibraryName>
                <otm:LibraryVersion>2.0.0</otm:LibraryVersion>
                <otm:SourceFile>/OTA2_BuiltIns_v2.0.0.otm</otm:SourceFile>
                <otm:CompilerVersion>3.0</otm:CompilerVersion>
                <otm:CompileDate>2014-01-10T13:29:42.553+01:00</otm:CompileDate>
            </otm:Library>
        </xsd:appinfo>
    </xsd:annotation>

    <xsd:import namespace="http://opentravel.org/common/message/v02" schemaLocation="OTA2_Message_v2.0.0.xsd"/>
    <xsd:import
            namespace="http://www.OpenTravel.org/ns/OTA2/AppInfo_v01_00" schemaLocation="OTA2_AppInfo_v1.0.0.xsd"/>

    <xsd:simpleType name="Empty">
        <xsd:annotation>
            <xsd:documentation source="Description">An empty string used when the simple content of a complex type or attribute is to be
                omitted.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Empty</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>

    <xsd:simpleType name="String_Alias">
        <xsd:annotation>
            <xsd:documentation source="Description">A core 2.0 simpleType used to constrain the contents of an Alias string.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">String_Alias</otm:OTA2Entity>
                <otm:Example context="OTM2">Alias_Name</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Za-z0-9\.\-_]*"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="String_EnumExtension">
        <xsd:annotation>
            <xsd:documentation source="Description">A string used for an open enumeration literal value.</xsd:documentation>
            <xsd:documentation source="Implementer">Place a literal in this attribute if you have selected the "other_" value in the
                enumerated list. Note that this value should be known to your trading partners.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">String_EnumExtension</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="128"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="String_Long">
        <xsd:annotation>
            <xsd:documentation source="Description">A string with a minimum length of 1 and a maximum length of 4096.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">String_Long</otm:OTA2Entity>
                <otm:Example context="OTM2">I am a long string.</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="4096"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="String_Medium">
        <xsd:annotation>
            <xsd:documentation source="Description">A string with a minimum length of 1 and a maximum length of 512.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">String_Medium</otm:OTA2Entity>
                <otm:Example context="OTM2">I am a medium string.</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="512"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="String_PaymentCard">
        <xsd:annotation>
            <xsd:documentation source="Description">A string that represents a payment card number up to 19 numeric characters in length.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">String_PaymentCard</otm:OTA2Entity>
                <otm:Example context="OTM2">1111222233334444</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{4,19}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="String_Phone">
        <xsd:annotation>
            <xsd:documentation source="Description">A string that represents a phone number with the phone number exchange and suffix.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">String_Phone</otm:OTA2Entity>
                <otm:Example context="OTM2">5551212</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{7}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="String_Short">
        <xsd:annotation>
            <xsd:documentation source="Description">A string with a minimum length of 1 and a maximum length of 128.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">String_Short</otm:OTA2Entity>
                <otm:Example context="OTM2">I am a short string.</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="128"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="String_Tiny">
        <xsd:annotation>
            <xsd:documentation source="Description">A string with a minimum length of 1 and a maximum length of 32.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">String_Tiny</otm:OTA2Entity>
                <otm:Example context="OTM2">I am a tiny string.</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="32"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="String_Alpha2">
        <xsd:annotation>
            <xsd:documentation source="Description">A 2 character alphanueric string.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">String_Alpha2</otm:OTA2Entity>
                <otm:Example context="OTM2">AB</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{2}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="String_Alpha3">
        <xsd:annotation>
            <xsd:documentation source="Description">A 3 character alphanueric string.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">String_Alpha3</otm:OTA2Entity>
                <otm:Example context="OTM2">ABC</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{3}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_AgentDuty">
        <xsd:annotation>
            <xsd:documentation source="Description">Typically a two to five character code (alpha, numeric, or combination) that specifies a
                travel agent duty code that is used for agency authentication.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_AgentDuty</otm:OTA2Entity>
                <otm:Example context="OTM2">AG</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z0-9]{2,5}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_ATPCO_PassengerType">
        <xsd:annotation>
            <xsd:documentation source="Description">An ATPCO passenger type code.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_ATPCO_PassengerType</otm:OTA2Entity>
                <otm:Example context="OTM2">MIL</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="ota2:String_Tiny"/>
    </xsd:simpleType>

    <xsd:simpleType name="Code_DataUsePolicy">
        <xsd:annotation>
            <xsd:documentation source="Description">A code that identifies a data use policy and/or restriction.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_DataUsePolicy</otm:OTA2Entity>
                <otm:Example context="OTM2">Internal Use</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="128"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_DayOfWeek">
        <xsd:annotation>
            <xsd:documentation source="Description">A three or four character abbreviation for a day of the week.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_DayOfWeek</otm:OTA2Entity>
                <otm:Example context="OTM2">WED</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z0-9]{3,4}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_IATA_AircraftType">
        <xsd:annotation>
            <xsd:documentation source="Description">A three character IATA aircraft type designator.</xsd:documentation>
            <xsd:documentation source="Reference">http://www.iata.org/ps/publications/pages/code-search.aspx</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_IATA_AircraftType</otm:OTA2Entity>
                <otm:Example context="OTM2">310</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="3"/>
            <xsd:maxLength value="3"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_IATA_AirlineID">
        <xsd:annotation>
            <xsd:documentation source="Description">A two character IATA assigned airline code.</xsd:documentation>
            <xsd:documentation source="Reference">https://en.wikipedia.org/wiki/IATA_aircraft_type_designator</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_IATA_AirlineID</otm:OTA2Entity>
                <otm:Example context="OTM2">UA</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{2}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_IATA_Airport">
        <xsd:annotation>
            <xsd:documentation source="Description">A three character code for an airport.</xsd:documentation>
            <xsd:documentation source="Reference">http://www.iata.org/ps/publications/pages/code-search.aspx</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_IATA_Airport</otm:OTA2Entity>
                <otm:Example context="OTM2">JFK</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{3}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_IATA_City">
        <xsd:annotation>
            <xsd:documentation source="Description">A three character code for a city.</xsd:documentation>
            <xsd:documentation source="Reference">http://www.iata.org/ps/publications/pages/code-search.aspx</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_IATA_City</otm:OTA2Entity>
                <otm:Example context="OTM2">NYC</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{3}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_IATA_ERSP">
        <xsd:annotation>
            <xsd:documentation source="Description">A unique IATA (in cooperation with ATA and ARC) code used as a means of identification
                when performing online bookings with airlines.
            </xsd:documentation>
            <xsd:documentation source="Reference">http://www.iata.org/ps/accreditation-travel/ersp/pages/index.aspx</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_IATA_ERSP</otm:OTA2Entity>
                <otm:Example context="OTM2">123456789</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{1,16}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_ISO_Country">
        <xsd:annotation>
            <xsd:documentation source="Description">A three character country code defined in ISO 3166 format.</xsd:documentation>
            <xsd:documentation source="Reference">http://www.iso.org/iso/country_codes/iso_3166_code_lists.htm</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_ISO_Country</otm:OTA2Entity>
                <otm:Example context="OTM2">USA</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{3}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_ISO_Currency">
        <xsd:annotation>
            <xsd:documentation source="Description">A three character currency code as defined in ISO 4217.</xsd:documentation>
            <xsd:documentation source="Reference">http://bit.ly/OTA2_ISO4217</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_ISO_Currency</otm:OTA2Entity>
                <otm:Example context="OTM2">USD</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{3}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_ISO_Language">
        <xsd:annotation>
            <xsd:documentation source="Description">A code for a language in ISO 639-1 (2 alpha character) format.</xsd:documentation>
            <xsd:documentation source="Reference">http://sharepoint.opentraveldevelopersnetwork.com/Lists/The OpenTravel
                Glossary/AllItems.aspx
            </xsd:documentation>
            <xsd:documentation source="Implementer">Language codes are typically used to describe spoken languages and languages that
                descriptive information is requested and/or provided in. An example is "en" which is English.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_ISO_Language</otm:OTA2Entity>
                <otm:Example context="OTM2">en</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{2}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_ProcessingCondition">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies a one to nine character length code that represents an abbreviated message
                processing condition.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_ProcessingCondition</otm:OTA2Entity>
                <otm:Example context="OTM2">NOAVAIL</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9a-zA-Z]{1,9}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_MoneyToPointsRatio">
        <xsd:annotation>
            <xsd:documentation source="Description">The ratio used to calculate the number of loyalty program points (or miles) required to
                pay for a monetary amount.
            </xsd:documentation>
            <xsd:documentation source="Implementer">For example, a base air fare of $100 where MoneyToPointsRatio is 1000 requires 100,000
                frequent flyer miles to pay for the full base fare (100 * 1000 = 100,000 miles.)
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_MoneyToPointsRatio</otm:OTA2Entity>
                <otm:Example context="OTM2">1000</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:decimal"/>
    </xsd:simpleType>

    <xsd:simpleType name="Code_OpenTravel">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies a one to three character length code that represents a value from an
                OpenTravel Code List.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_OpenTravel</otm:OTA2Entity>
                <otm:Example context="OTM2">123</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{1,3}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_OSI">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies an other service request (OSI) code that may be associated with a passenger
                name record (PNR).
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_OSI</otm:OTA2Entity>
                <otm:Example context="OTM2">YY</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Z]{2,4}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_PaymentCardSeries">
        <xsd:annotation>
            <xsd:documentation source="Description">Verification digits printed on a payment card that follow the embossed number.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_PaymentCardSeries</otm:OTA2Entity>
                <otm:Example context="OTM2">123456789</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{2,9}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_PaymentCardType">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies a 2 to 9 character alphanumeric code for a payment card.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_PaymentCardType</otm:OTA2Entity>
                <otm:Example context="OTM2">MC</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{2,9}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_Phone_Area">
        <xsd:annotation>
            <xsd:documentation source="Description">A code that represents the area code in a telephone number.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_Phone_Area</otm:OTA2Entity>
                <otm:Example context="OTM2">203</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{3}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_Phone_Country">
        <xsd:annotation>
            <xsd:documentation source="Description">A code that represents a country in a telephone number.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_Phone_Country</otm:OTA2Entity>
                <otm:Example context="OTM2">44</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{1,3}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_Postal">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies a postal code, known in various countries as a post code, postcode or ZIP
                code.
            </xsd:documentation>
            <xsd:documentation source="Reference">http://en.wikipedia.org/wiki/List_of_postal_codes</xsd:documentation>
            <xsd:documentation source="Implementer">A postal code contains a series of letters and/or digits appended to a postal address
                for the purpose of sorting mail.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_Postal</otm:OTA2Entity>
                <otm:Example context="OTM2">33626</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="16"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_Promotion">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies a promotion code.</xsd:documentation>
            <xsd:documentation source="Implementer">A promotion code is the identifier used by the host system to link directly with a
                specific named advertising campaign. By including the required code, the client system is able to gain access to special
                offers which may have been created for a specifically targeted group via a CRM system or for a wider advertising campaign
                using television or press advertisements.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_Promotion</otm:OTA2Entity>
                <otm:Example context="OTM2">TWOFORONE</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="ota2:String_Tiny"/>
    </xsd:simpleType>

    <xsd:simpleType name="Code_PseudoCity">
        <xsd:annotation>
            <xsd:documentation source="Description">A three to five character code (alpha, numeric or combination) that specifies a GDS code
                used to identify a travel agency location.
            </xsd:documentation>
            <xsd:documentation source="Reference">http://sharepoint.opentraveldevelopersnetwork.com/Lists/The OpenTravel
                Glossary/AllItems.aspx
            </xsd:documentation>
            <xsd:documentation source="Implementer">This code is typically required when using negotiated rates that are tied to an agency
                through this code.
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_PseudoCity</otm:OTA2Entity>
                <otm:Example context="OTM2">DFW</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z0-9]{3,5}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_RatePlan">
        <xsd:annotation>
            <xsd:documentation source="Description">A code that identifies a rate plan.</xsd:documentation>
            <xsd:documentation source="Reference">http://sharepoint.opentraveldevelopersnetwork.com/Lists/The OpenTravel
                Glossary/AllItems.aspx
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_RatePlan</otm:OTA2Entity>
                <otm:Example context="OTM2">RoomAndMeal</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="64"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_Region">
        <xsd:annotation>
            <xsd:documentation source="Description">Up to a 16 character code for a geographic region.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_Region</otm:OTA2Entity>
                <otm:Example context="OTM2">Europe</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Za-z0-9]{1,16}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_SSR">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies a special request (SSR) code associated with a passenger name record (PNR).
            </xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_SSR</otm:OTA2Entity>
                <otm:Example context="OTM2">BLND</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Z]{4}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_State">
        <xsd:annotation>
            <xsd:documentation source="Description">A 2 character abbreviation for a state.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_State</otm:OTA2Entity>
                <otm:Example context="OTM2">CA</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Za-z]{2}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Code_Tour">
        <xsd:annotation>
            <xsd:documentation source="Description">Up to a 32 character numeric or alphanumeric tour code.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">Code_Tour</otm:OTA2Entity>
                <otm:Example context="OTM2">GOGOEUROPE</otm:Example>
            </xsd:appinfo>
